  <thing
    id="fEpcArmSup"
    name=""
    compset="">
    <eval index="1" phase="Traits" priority="10000">
      <before name="Armor effects final"/>
      <before name="Derived trtFinal"/>
      <after name="arMagic copy"/>
      <![CDATA[
      doneif (activated = 0)

      perform hero.findchild[Armor,"ArmorCat.Heavy & Equipped.Equipped"].setfocus
      doneif (state.isfocus = 0)
      #traitmodify[defAC,trtBonus,2,""]
      focus.field[arCheck].value = 0
      focus.field[arSpeed].value = 0
      ]]></eval>
    </thing>
