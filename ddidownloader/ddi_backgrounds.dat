<document signature="Hero Lab Data">

  <thing
    id="bgAkanul"
    name=""
    compset="">
    <eval index="1" phase="Traits" priority="1000"><![CDATA[
      var resist as number
      if (#level[] <= 10) then
        resist = 2
      elseif (#level[] <= 20) then
        resist = 3
      else
        resist = 5
        endif

      ~cold / fire / thunder +2 / +3 / +5
      #traitmodify[rsCold,rsExtra,resist,""]
      #traitmodify[rsFire,rsExtra,resist,""]
      #traitmodify[rsThunder,rsExtra,resist,""]
      ]]></eval>
    </thing>

