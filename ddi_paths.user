    <bootstrap thing="pParWarWra"></bootstrap>
    </thing>
  <thing id="fParDetAct" name="Determined Action" description="You can spend an action point during your turn as a free action to regain a number of hit points equal to one-half your level + your Wisdom modifier instead of gaining the extra action." compset="ClassFeat">
