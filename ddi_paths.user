  <thing id="fParImpIro" name="Impenetrable Iron" description="Whenever a critical hit is scored against you, roll a d20. On a roll of 10 or higher, the critical hit is a normal hit instead." compset="ClassFeat">
    <tag group="ReqLevel" tag="16"/>
