    <bootstrap thing="pParRadVis"></bootstrap>
    </thing>
  <thing id="fParSoaCha" name="Soaring Charge" description="When you charge, you can fly your speed as part of the charge. You also gain a +2 bonus to all defenses against opportunity attacks you provoke while moving during the charge." compset="ClassFeat">
