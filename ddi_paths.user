    <bootstrap thing="pParBldFry"></bootstrap>
    <prereq message="&apos;Rampage&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fBbnRampag"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="fParBatSpi" name="Battle Spirit&apos;s Touch" description="When you spend an action point to make an attack and hit at least one target with that attack, you can spend a healing surge." compset="ClassFeat">
