    <bootstrap thing="fParMasHun"></bootstrap>
    <bootstrap thing="fParHntAct"></bootstrap>
    <bootstrap thing="fParImpDsS"></bootstrap>
    <bootstrap thing="pParHunTra"></bootstrap>
    </thing>
  <thing id="fParHntAct" name="Hunter&apos;s Action" description="When you spend an action point to make an attack, you can reroll each attack roll you make before the end of your turn, and you must use the second roll each time." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParImpDsS" name="Level 11 Improved Disruptive Shot" description="You gain one more use of disruptive shot per encounter, but you can still use it only once per turn." compset="ClassFeat">
