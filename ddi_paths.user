    <bootstrap thing="pParChaClo"></bootstrap>
    <bootstrap thing="pParStoRai"></bootstrap>
    <bootstrap thing="pParSwoFal"></bootstrap>
    </thing>
  <thing id="fParFocPar" name="Focused Parry Action" description="When you spend an action point to make an attack and the attack hits, you gain a bonus to all defenses equal to your Constitution modifier until you make an attack or until the end of the encounter." compset="ClassFeat">
