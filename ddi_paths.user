    </thing>
  <thing id="fParStaAss" name="Stalwart Assault" description="During your first turn in an encounter, you gain a bonus to speed and melee weapon damage rolls equal to your Constitution modifier." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
