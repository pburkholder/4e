  <thing id="fParItiAct" name="Itinerant Action" description="When an attack hits you, you can spend an action point as an immediate interrupt to gain a +4 bonus to all defenses until the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
