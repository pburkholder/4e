    <bootstrap thing="pParDemBol"></bootstrap>
    </thing>
  <thing id="fParDemFur" name="Demon Fury" description="When you spend an action point to take an extra action, you and your allies gain a +3 bonus to attack rolls until the start of your next turn, and your enemies gain a +3 bonus to their attack rolls against you until the start of your next turn." compset="ClassFeat">
