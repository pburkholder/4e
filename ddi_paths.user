  <thing id="fParDrcRst" name="Draconic Resistance" description="Choose one damage type you can deal with your dragon breath power. You gain resistance to that damage type equal to 5 + one-half your level." compset="ClassFeat">
    <fieldval field="usrCandid1" value="component.Resistance"/>
