    <bootstrap thing="pParDraVen"></bootstrap>
    <bootstrap thing="pParDraAva"></bootstrap>
    <bootstrap thing="pParDraTor"></bootstrap>
    </thing>
  <thing id="fParStrDra" name="Strength of the Dragon" description="You gain resist 5 fire, or if you already have fire resistance, it increases by 5. Whenever you take fire damage, you gain a +2 bonus to damage rolls until the end of your next turn, even if your fire resistance negates the damage." compset="ClassFeat">
