    <bootstrap thing="fParRapAct"></bootstrap>
    <bootstrap thing="pParPerAim"></bootstrap>
    <bootstrap thing="pParEscBar"></bootstrap>
    <bootstrap thing="pParStaSho"></bootstrap>
    </thing>
  <thing id="fParOppFir" name="Opportunity Fire" description="When an adjacent creature provokes an opportunity attack from you while you are wielding a bow or loaded crossbow, you can make a ranged basic attack against the provoking creature as an opportunity action. This attack does not provoke opportunity attacks." compset="ClassFeat">
