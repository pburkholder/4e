      <validate><![CDATA[var featureid as string
featureid = "fRgrBeaMas"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="fParNecSee" name="Neck Seeker" description="You and your beast companion gain a +1 bonus to attack rolls and damage rolls against any bloodied enemy designated as your quarry." compset="ClassFeat">
