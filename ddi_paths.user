    <bootstrap thing="fParShaGod"></bootstrap>
    <bootstrap thing="fParHarbin"></bootstrap>
    <bootstrap thing="pParPreGai"></bootstrap>
    <bootstrap thing="pParCovAss"></bootstrap>
    <bootstrap thing="pParWitOns"></bootstrap>
    </thing>
  <thing id="fParBleDef" name="Bleeding Defeat Action" description="When you spend an action point to make an attack, if that attack hits and deals damage, the target also takes ongoing 10 radiant and necrotic damage (save ends)." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParShaGod" name="Shadow of the Gods" description="Once per turn when you deal necrotic damage or radiant damage to an enemy with an assassin attack, an avenger attack, or a covenant agent attack, the target of the damage also takes a -3 penalty to the next saving throw it makes before the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
