  <thing id="fParEneEss" name="Energy Essence" description="If you hit with an arcane attack power that deals more than one type of damage, you deal 1d6 extra damage." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParEssAct" name="Essential Action" description="When you spend an action point to take an extra action, you gain a +4 bonus to attack rolls with arcane powers until the start of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
