    <bootstrap thing="pParSusHar"></bootstrap>
    </thing>
  <thing id="fParDrvnAc" name="Driven Action" description="When you spend an action point to give a special command that allows a summoned creature to make an attack, that attack gains a power bonus to the attack roll equal to your Charisma modifier, and each enemy hit by the attack takes 5 extra damage." compset="ClassFeat">
