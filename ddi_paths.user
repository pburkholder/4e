    <bootstrap thing="pParTriRew"></bootstrap>
    <bootstrap thing="pParNimFal"></bootstrap>
    <bootstrap thing="pParLyiLig"></bootstrap>
    </thing>
  <thing id="fParShrAct" name="Shrouding Action" description="When you spend an action point to take an extra action, you and one ally you choose within 5 squares of you become invisible until the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
