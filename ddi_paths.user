    <bootstrap thing="pParBloShi"></bootstrap>
    <bootstrap thing="pParDriGor"></bootstrap>
    <bootstrap thing="pParFolGor"></bootstrap>
    </thing>
  <thing id="fParAggRes" name="Aggressive Resurgence" description="When you use your second wind, you can forgo regaining hit points and the bonus to your defenses to instead both regain the use of your goring charge racial power and gain a +2 bonus to attack rolls and damage rolls until the end of your next turn." compset="ClassFeat">
