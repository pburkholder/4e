      <validate><![CDATA[var featureid as string
featureid = "fSmgAegAss"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="fParAssCha" name="Assaulting Charge" description="When you spend an action point to charge, you can use a melee at-will attack power or a melee encounter attack power in place of the charge&apos;s melee basic attack." compset="ClassFeat">
