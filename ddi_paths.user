    <bootstrap thing="pParDeaDup"></bootstrap>
    <bootstrap thing="pParImiStr"></bootstrap>
    </thing>
  <thing id="fParCooAct" name="Coordinated Action" description="If you have not already spent an action point in the encounter, when an ally spends an action point to take an extra action, you can spend an action point as an immediate reaction. Your action must be taken after your ally has resolved the action from his or her action point." compset="ClassFeat">
