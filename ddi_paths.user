    <bootstrap thing="pParUnbAss"></bootstrap>
    </thing>
  <thing id="fParEagAct" name="Eager Action" description="When you spend an action point to take an extra action, you also gain a bonus to speed equal to your Dexterity modifier until the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
