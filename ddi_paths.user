    <bootstrap thing="fParAllEye"></bootstrap>
    <bootstrap thing="fParCerAct"></bootstrap>
    <bootstrap thing="fParCerSig"></bootstrap>
    <bootstrap thing="pParHarMin"></bootstrap>
    <bootstrap thing="pParSeaThr"></bootstrap>
    <bootstrap thing="pParCerBla"></bootstrap>
    </thing>
  <thing id="fParCerAct" name="Cerulean Action" description="When you spend an action point to use an at-will attack power that has a 1-point augmentation, you can use that augmentation without spending a power point." compset="ClassFeat">
