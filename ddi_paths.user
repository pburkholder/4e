  <thing id="fParWeaTra" name="Weapon Training" description="You gain proficiency with one heavy blade of your choice." compset="ClassFeat">
    <fieldval field="usrCandid1" value="component.WeaponBase &amp; WepGroup.wgHeavyBl"/>
