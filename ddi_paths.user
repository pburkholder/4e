  <thing id="fParEmpBon" name="Empathic Bond" description="Any time a power you use heals an ally, you also regain a number of hit points equal to the higher of your Wisdom modifier or Charisma modifier." compset="ClassFeat">
    <tag group="ReqLevel" tag="16"/>
