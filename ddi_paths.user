      foreach pick in hero from WeaponBase where expr
        perform eachpick.field[wpDamage].modify[+,1,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="fParKenMas" name="Kensei Mastery" description="You gain a +4 bonus to damage rolls with the same weapon you selected for Kensei Focus. If you ever use a different kind of weapon, you lose this benefit, and the benefit of Kensei Focus, until you take a short rest, during which time you reattune yourself to your chosen weapon with a short meditation." compset="ClassFeat">
