    <bootstrap thing="pParDriBef"></bootstrap>
    <prereq message="&apos;Battlerager Vigor&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fFtrBatVig"
call ReqFeature]]></validate>
      </prereq>
    </thing>
