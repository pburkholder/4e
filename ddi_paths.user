    <bootstrap thing="pParTosClo"></bootstrap>
    </thing>
  <thing id="fParInThi" name="In the Thick of the Action" description="When you spend an action point to take an extra action, you deal 4 extra damage against Large or larger enemies and gain +2 speed until the start of your next turn." compset="ClassFeat">
