    <bootstrap thing="fParThrMas"></bootstrap>
    <bootstrap thing="pParQuiTri"></bootstrap>
    <bootstrap thing="pParEruSte"></bootstrap>
    <bootstrap thing="pParLanStr"></bootstrap>
    </thing>
  <thing id="fParThrAct" name="Throwing Action" description="When you spend an action point to take an extra action, you can make a ranged basic attack with a light thrown weapon or a heavy thrown weapon as a free action. This ranged attack does not provoke opportunity attacks. You can then take an extra action." compset="ClassFeat">
