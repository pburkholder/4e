    <bootstrap thing="pParBeaWou"></bootstrap>
    <bootstrap thing="pParMarCry"></bootstrap>
    <bootstrap thing="pParSoOth"></bootstrap>
    </thing>
  <thing id="fParComBle" name="Compassionate Blessing" description="When you let an ally spend a healing surge to regain hit points using a healing power, you can choose to take 5 damage. If you do, one of the power&apos;s targets regains 2d6 additional hit points, and you gain a +2 power bonus to all defenses until the end of your next turn." compset="ClassFeat">
