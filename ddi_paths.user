    <bootstrap thing="fParRadFis"></bootstrap>
    <bootstrap thing="fParRadHam"></bootstrap>
    <bootstrap thing="pParUndSun"></bootstrap>
    <bootstrap thing="pParBliSun"></bootstrap>
    <bootstrap thing="pParDivSun"></bootstrap>
    </thing>
  <thing id="fParChaDiv" name="Channel Divinity - Radiant Fist" description="You gain the Channel Divinity class feature, which means you can use a single Channel Divinity power once per encounter. You also gain a divinity feat of your choice." compset="ClassFeat">
