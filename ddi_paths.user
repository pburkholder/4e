    <bootstrap thing="fParTurTid"></bootstrap>
    <bootstrap thing="pParBatRes"></bootstrap>
    <bootstrap thing="pParVicPra"></bootstrap>
    <bootstrap thing="pParFanEne"></bootstrap>
    </thing>
  <thing id="fParPrgAct" name="Paragon&apos;s Action" description="When you spend an action point to take an extra action, you gain one of the following benefits of your choice.{br} You or an ally within 5 squares of you can make a saving throw with a +5 bonus.{br} You or an ally within 5 squares of you gains a +5 power bonus to damage rolls until the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParTurTid" name="Turning of the Tide" description="Whenever you drop a nonminion enemy below 1 hit point with an attack, you gain one of the following benefits of your choice.{br} Each enemy within 2 squares of you is pushed up to 3 squares.{br} You or one ally within 5 squares of you gains 10 temporary hit points." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParRelReb" name="Relentless Rebuke" description="Once per encounter, when an enemy&apos;s attack against an ally within 5 squares of you bloodies that ally or drops that ally below 1 hit point, you can make a melee basic attack as an opportunity action." compset="ClassFeat">
    <tag group="ReqLevel" tag="16"/>
    <tag group="PowerUse" tag="Encounter"/>
    </thing>
  <thing id="ppPathfind" name="Pathfinder" description="You become the ultimate scout, finding a way through any situation. Sometimes you find the best path, but other times you have to carve that path out of the wilderness with your two blades singing through the air.{br}{br}PATHFINDER  PATH FEATURES{b}Battlehoned (11th level){/b}: You gain two extra healing surges per day.{br}{b}Pathfinder&apos;s Action (11th level){/b}: When you spend an action point to take an extra action, you also gain a move action.{br}{b}Cruel Recovery (16th level){/b}: When you hit an enemy and it takes damage from Hunter&apos;s Quarry, you gain temporary hit points equal to the amount of damage you dealt from Hunter&apos;s Quarry + your Wisdom modifier." compset="Paragon" uniqueness="unique">
    <fieldval field="pptFlavor" value="&amp;quot;I can find us a path through that maze of horror, and I can safely lead us to the other end.&amp;quot;"/>
    <fieldval field="reqText" value="Ranger class, two-blade fighting style"/>
