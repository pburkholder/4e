    <bootstrap thing="fParUnsKil"></bootstrap>
    <bootstrap thing="pParFoaWra"></bootstrap>
    <bootstrap thing="pParManFur"></bootstrap>
    <bootstrap thing="pParMutStr"></bootstrap>
    </thing>
  <thing id="fParBldAct" name="Blood-Crazed Action" description="When you spend an action point to use an attack power, your charge attacks gain a +2 power bonus to attack rolls and a +4 power bonus to damage rolls until the end of your next turn." compset="ClassFeat">
