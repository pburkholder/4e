    <bootstrap thing="pParDeaMar"></bootstrap>
    <bootstrap thing="pParInsSuc"></bootstrap>
    </thing>
  <thing id="fParClito" name="Cling to Your Words" description="Whenever an ally in your skald&apos;s aura drops below 1 hit point, you can use the aura&apos;s healing benefit on that ally as an immediate reaction, but only if the aura&apos;s healing benefit has uses remaining." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParSkaAct" name="Skald&apos;s Action" description="When you spend an action point on your turn, you can forgo taking an extra action to allow one ally in your skald&apos;s aura to take a standard action as a free action." compset="ClassFeat">
