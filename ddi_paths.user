    <bootstrap thing="pParGuiSyl"></bootstrap>
    <bootstrap thing="pParWorSpo"></bootstrap>
    <bootstrap thing="pParSylLig"></bootstrap>
    </thing>
  <thing id="fParInfSyl" name="Influencing Syllable" description="You gain a +2 bonus to Charisma-based skill checks." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParSpeAct" name="Speaker&apos;s Action" description="When you spend an action point to take an extra action, you can also reroll one die you roll during the same turn for an attack roll, a damage roll, a skill check, or a saving throw and use either result." compset="ClassFeat">
