    <bootstrap thing="pParAppTra"></bootstrap>
    <bootstrap thing="pParOthPul"></bootstrap>
    <exprreq message="Feat &apos;Foulborn Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftFoulbHer] <> 0]]></exprreq>
    </thing>
  <thing id="fParTerAct" name="Terrifying Action" description="When you spend an action point to take an extra action, you mark any enemy adjacent to you until the end of your next turn." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParUnnPer" name="Unnatural Perception" description="You gain a +2 bonus to Insight checks, Perception checks, and initiative checks. You also have blindsight 5 while you are blinded." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
