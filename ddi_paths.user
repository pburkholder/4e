    <bootstrap thing="pParMesWar"></bootstrap>
    <bootstrap thing="pParTruAwe"></bootstrap>
    </thing>
  <thing id="fParGifGra" name="Gift of Grace Action" description="When you spend an action point to take an extra action, each ally adjacent to you also gains a bonus to all defenses equal to your Charisma modifier until the end of your next turn." compset="ClassFeat">
