    <bootstrap thing="fParHinGra"></bootstrap>
    <bootstrap thing="pParDanFla"></bootstrap>
    <bootstrap thing="pParGraSma"></bootstrap>
    <bootstrap thing="pParEnsCha"></bootstrap>
    <exprreq message="Feat &apos;Rending Chains Warmaster&apos; required"><![CDATA[hero.tagis[Feat.ftRendiWar] <> 0]]></exprreq>
    </thing>
