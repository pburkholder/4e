  <thing id="fParComIns" name="Compensatory Insight" description="When you use an attack power granted through a multiclass feat, you gain a bonus to the attack&apos;s damage roll equal to your Intelligence modifier." compset="ClassFeat">
    <tag group="ReqLevel" tag="16"/>
