    <bootstrap thing="pParEyeBas"></bootstrap>
    </thing>
  <thing id="fParBalAct" name="Baleful Action" description="You can spend an action point to slow each enemy within 5 squares of you that can see you until the start of your next turn. Any enemy that takes damage while slowed in this way becomes slowed (save ends)." compset="ClassFeat">
