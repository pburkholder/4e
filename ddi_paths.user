    <bootstrap thing="pParBlaCur"></bootstrap>
    </thing>
  <thing id="fParBlaAct" name="Blackstone Action" description="When you spend an action point to use an attack power, you gain resistance to all damage equal to 3 + your Wisdom modifier until the end of your next turn." compset="ClassFeat">
