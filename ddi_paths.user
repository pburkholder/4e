    <bootstrap thing="fParSerEdg"></bootstrap>
    <bootstrap thing="pParDeaCal"></bootstrap>
    <bootstrap thing="pParHeaEdg"></bootstrap>
    <bootstrap thing="pParDefCut"></bootstrap>
    </thing>
  <thing id="fParCalEye" name="Calm Eye" description="When you spend an action point to use an at-will attack power, that attack deals 2d10 extra damage." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParSerEdg" name="Serene Edge" description="When you miss your oath of enmity target with an encounter attack power, the attack deals damage to the target equal to your Dexterity modifier." compset="ClassFeat">
