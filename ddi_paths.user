    <bootstrap thing="pParShrAde"></bootstrap>
    <bootstrap thing="pParUltThi"></bootstrap>
    <bootstrap thing="pParHekTra"></bootstrap>
    </thing>
  <thing id="fParMadAct" name="Maddening Action" description="When you spend an action point to take an extra action, one creature within 5 squares of you takes ongoing 5 psychic damage (save ends)." compset="ClassFeat">
