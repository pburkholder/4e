      <validate>perform hero.childfound[pRacShaSwa].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="fParPsyNet" name="Psychic Network" description="Choose two skills: Arcana, Dungeoneering, History, Nature, or Religion. You become trained in those skills." compset="ClassFeat">
    <fieldval field="usrCandid1" value="component.Skill &amp; (thingid.skArcana | thingid.skDungeone | thingid.skHistory | thingid.skNature | thingid.skReligion)"/>
