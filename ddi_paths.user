    <bootstrap thing="fParIncPre"></bootstrap>
    <bootstrap thing="fParBriOve"></bootstrap>
    <bootstrap thing="fParLumAct"></bootstrap>
    <bootstrap thing="pParUnbCou"></bootstrap>
    <bootstrap thing="pParPerInc"></bootstrap>
    <bootstrap thing="pParIncStr"></bootstrap>
    </thing>
  <thing id="fParBriOve" name="Brilliant Overload" description="Whenever you hit an enemy with an augmented psionic at-will attack power, you can spend 1 power point to have the power deal 1d6 extra radiant damage." compset="ClassFeat">
