    <bootstrap thing="pParHelRep"></bootstrap>
    </thing>
  <thing id="fParHelRet" name="Hellfire Retribution" description="Whenever an enemy marked by you makes an attack that doesn&apos;t include you as a target, that enemy takes fire damage equal to 3 + your Constitution modifier." compset="ClassFeat">
