    <bootstrap thing="pParLifShi"></bootstrap>
    <bootstrap thing="pParLigLos"></bootstrap>
    <bootstrap thing="pParVitSpe"></bootstrap>
    </thing>
  <thing id="fParBleLan" name="Blessing of the Land" description="You can no longer use arcane defiling. In addition, you gain a +2 bonus to Endurance, Nature, and Perception checks." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParMasPre" name="Master Preserver&apos;s Action" description="When you spend an action point to make an arcane attack, you can choose to reroll one attack roll or damage roll and use either result." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
