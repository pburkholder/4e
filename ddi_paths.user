      <validate><![CDATA[var featureid as string
featureid = "fBbnWhiSla"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="fParCloDes" name="Close Destruction" description="When you spend an action point to make an attack, you gain a bonus to its attack rolls equal to the number of enemies adjacent to you." compset="ClassFeat">
