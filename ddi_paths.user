  <thing id="fParDagPre" name="Dagger Precision" description="When you use a rogue or a daggermaster attack power with a dagger, the power can score a critical hit on a roll of 18-20." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="fParDagAct" name="Daggermaster&apos;s Action" description="You can spend an action point to reroll an attack roll or a damage roll you make using a dagger, instead of taking an extra action." compset="ClassFeat">
