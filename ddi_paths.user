    <bootstrap thing="pParCloSol"></bootstrap>
    </thing>
  <thing id="fParCloCoo" name="Clockwork Coordinated Action" description="When you spend an action point to take an extra action, you also gain two minor actions, which you can use only to command your summoned creatures." compset="ClassFeat">
