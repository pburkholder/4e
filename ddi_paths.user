    </thing>
  <thing id="fParCarTra" name="Careful Trap-Breaker" description="You gain a +5 bonus to Thievery checks to delay a trap, and you do not trigger a trap with a failed Thievery check." compset="ClassFeat">
    <tag group="ReqLevel" tag="11"/>
