  <thing id="ftScrappy" name="Scrappy" description="{b}Benefit{/b}: When wielding a one-handed versatile weapon in two hands, you gain a +1 bonus to damage rolls. This bonus increases to +2 at 11th level and +3 at 21st level." compset="Feat" uniqueness="useronce">
