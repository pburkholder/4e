  <thing id="ftImproSmW" name="Improved Swordmage Warding" description="{b}Benefit{/b}: While you are wearing cloth armor, leather armor, or no armor, the bonus to AC from your Swordmage Warding increases by 1." compset="Feat" uniqueness="useronce">
