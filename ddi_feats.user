  <thing id="ftNightKil" name="Nightmare Killer" description="{b}Benefit{/b}: You gain a +2 feat bonus to damage rolls with fear powers. The bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
