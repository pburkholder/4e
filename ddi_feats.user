  <thing id="ftWallCra" name="Wall Crawlers" description="{b}Benefit{/b}: You gain a +2 feat bonus to Athletics checks. Whenever an ally with this feat successfully aids you on your Athletics check, you gain a +4 bonus instead of the normal +2 bonus. An ally with this feat can aid you by taking the same type of action you are taking to use Athletics." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
