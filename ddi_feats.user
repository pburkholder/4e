    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBard"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skArcana,trtFeat,2,""]
      #traitmodify[skDungeone,trtFeat,2,""]
      #traitmodify[skHistory,trtFeat,2,""]
      #traitmodify[skNature,trtFeat,2,""]
      #traitmodify[skReligion,trtFeat,2,""]
      #traitmodify[skStreetwi,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftBardiRit" name="Bardic Ritualist" description="{b}Benefit{/b}: You gain training in the Arcana skill. You gain the bard&apos;s Bardic Training class feature. In addition, you can wield bard implements." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Int 13, Cha 13"/>
    <fieldval field="reqInt" value="13"/>
    <fieldval field="reqCha" value="13"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="clsBard"/>
