  <thing id="ftSureCli" name="Sure Climber" description="{b}Benefit{/b}: A successful Athletics check allows you to climb at your normal speed, rather than half speed.{br}You also gain a +1 feat bonus to Athletics checks." compset="Feat" uniqueness="useronce">
