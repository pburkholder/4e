    <tag group="ReqDeity" tag="dtyBalinor"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtBaliPre"></bootstrap>
    </thing>
  <thing id="ftBanesIns" name="Bane&apos;s Instructive Stratagem" description="{b}Benefit{/b}: You gain the power Bane&apos;s instructive stratagem." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Bane"/>
