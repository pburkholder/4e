  <thing id="ftBardImp" name="Bard Implement Expertise" description="{b}Benefit{/b}: When you wield a bard implement while using an arcane power, you can score a critical hit on a roll of 19-20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Con 15, Int 15, Cha 21, bard"/>
    <fieldval field="reqCon" value="15"/>
    <fieldval field="reqInt" value="15"/>
    <fieldval field="reqCha" value="21"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsBard"/>
    </thing>
  <thing id="ftBardAll" name="Bard of All Trades" description="{b}Benefit{/b}: You gain a +3 feat bonus to all untrained skill checks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Bard"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBard"/>
    <eval phase="Traits" priority="23000"><![CDATA[
      foreach pick in hero from Skill where "!Helper.Trained?"
        perform eachpick.field[trtFeat].modify[+,3,""]
        nexteach]]>
      <before name="Derived trtFinal"/>
      <after name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftBardiDil" name="Bardic Dilettante" description="{b}Benefit{/b}: You gain training in one skill from the bard&apos;s class skills list. Once per day, you can use the bard&apos;s majestic word power.{br}In addition, you can wield bard implements." compset="Feat">
    <fieldval field="reqText" value="Cha 13"/>
    <fieldval field="reqCha" value="13"/>
    <fieldval field="usrCandid1" value="component.Skill &amp; !(thingid.skEnduranc | thingid.skStealth | thingid.skThievery)"/>
    <usesource source="PHB2"/>
