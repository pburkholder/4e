  <thing id="ftArSpCha" name="Armor Specialization (Chainmail)" description="{b}Benefit{/b}: You gain a +1 feat bonus to AC while wearing chainmail. You reduce the check penalty incurred by chainmail by 1." compset="Feat" uniqueness="useronce">
