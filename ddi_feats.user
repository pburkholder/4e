    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="Multiclass"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="10"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtStraHol"></bootstrap>
    <exprreq message="Feat &apos;Garrote Training&apos; required"><![CDATA[hero.tagis[Feat.ftGarroTra] <> 0]]></exprreq>
    </thing>
  <thing id="ftGarroTra" name="Garrote Training" description="{b}Benefit{/b}: You gain proficiency with the garrote. When you use a garrote to successfully grab a target, the target takes a -2 penalty to escape the grab. Each turn you successfully maintain a grab with a garrote used with two hands, you have combat advantage against the target you&apos;re grabbing for the purpose of attacking again with the garrote.{br}If you are a rogue, you can use the garrote with the Sneak Attack class feature and any rogue melee attack power that can be used with a light blade." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 13, Dex 13"/>
    <fieldval field="reqStr" value="13"/>
    <fieldval field="reqDex" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="WepMastery"/>
    <eval phase="Traits" priority="1000">perform hero.assign[WeaponProf.wpGarrote]
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftGatheNig" name="Gathering Night" description="{b}Benefit{/b}: Whenever you take the total defense action while you have any concealment, you become invisible until the start of your next turn." compset="Feat" uniqueness="useronce">
