    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="ArenaFight" name="Arena Fighting"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    </thing>
  <thing id="ftBattlDuu" name="Battlemind Duulora Initiation" description="{b}Benefit{/b}: When you use speed of thought, you mark each enemy adjacent to you at the end of the move until the end of its next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Kalashtar, battlemind, speed of thought class feature"/>
