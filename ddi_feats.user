  <thing id="ftVengeRec" name="Vengeance Recalled" description="{b}Benefit{/b}: When you miss with an avenger power against your oath of enmity target, you gain a +5 bonus to damage rolls against that target until the end of your next turn." compset="Feat" uniqueness="useronce">
