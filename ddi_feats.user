    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 5
      elseif (#level[] <= 20) then
        bonus = 10
      else
        bonus = 15
        endif
      #traitmodify[rsNecrotic,rsTotal,bonus,""]]]></eval>
    <exprreq message="Feat &apos;Death&apos;s Blessing&apos; required"><![CDATA[hero.tagis[Feat.ftDeathBle] <> 0]]></exprreq>
    </thing>
  <thing id="ftReapeStr" name="Reaper&apos;s Strike" description="{b}Benefit{/b}: When you use your dark reaping racial power, you also gain a +2 bonus to your next attack roll." compset="Feat" uniqueness="useronce">
