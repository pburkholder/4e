      <validate><![CDATA[var featureid as string
featureid = "fArtHeaInf"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftFurioDem" name="Furious Demand" description="{b}Benefit{/b}: You gain the furious demand feat power, which replaces a utility power of 2nd level or higher.{br}In addition, you gain a +2 feat bonus to Intimidate checks." compset="Feat" uniqueness="useronce">
