  <thing id="ftHeavyArm" name="Heavy Armor Agility" description="{b}Benefit{/b}: You ignore the speed penalty for wearing heavy armor." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Strength 15 or Constitution 15"/>
