    <tag group="ReqRace" tag="rHalfling"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    <prereq message="&apos;Wild Magic&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fSorWilMag"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftTriumAtt" name="Triumphant Attack" description="{b}Benefit{/b}: If you score a critical hit with a melee attack, the target of your attack takes a -2 penalty to attack rolls and defenses for the rest of the encounter." compset="Feat" uniqueness="useronce">
