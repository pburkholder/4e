    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGnome"/>
    <tag group="ReqSkill" tag="skBluff"/>
    </thing>
  <thing id="ftGhostSco" name="Ghost Scorpion Strike" description="{b}Benefit{/b}: Your necrotic attack powers and poison attack powers ignore the insubstantial quality." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsShadow"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftGhostTou" name="Ghost Touched" description="{b}Benefit{/b}: You gain a +2 bonus to Religion checks. In addition, you gain a benefit with any of the following powers you possess.{br}Holy Strike: If your target is undead, one ally adjacent to you gains a bonus to its next damage roll against the target equal to your Wisdom modifier.{br}Magic Missile: When using this spell against an undead enemy that is insubstantial, your attack ignores the insubstantial trait, dealing full damage instead of half damage.{br}Sacred Flame: Whenever you use this prayer against an undead creature, you can choose to grant one ally resist necrotic equal to your Charisma modifier instead of granting temporary hit points or the ability to make a saving throw." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Con 13, Int 13"/>
    <fieldval field="reqCon" value="13"/>
    <fieldval field="reqInt" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftGhostRej" name="Ghostly Rejuvenation" description="{b}Benefit{/b}: When you use a power with the Healing keyword on an ally, one target of that power can also become insubstantial until the end of its next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, shadar-kai"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rShadarkai"/>
    </thing>
  <thing id="ftGhostVit" name="Ghostly Vitality" description="{b}Benefit{/b}: If you drop to 0 or fewer hit points and choose to remain conscious due to Unnatural Vitality, you can take a move action and a minor action in addition to the standard action Unnatural Vitality allows. While you have 0 or fewer hit points, you gain insubstantial." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, revenant"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqRace" tag="rRevenant"/>
    </thing>
  <thing id="ftGhostVoy" name="Ghostly Voyage" description="{b}Benefit{/b}: When you use voyage of the ancients, you also become insubstantial until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wilden, assassin, voyage of the ancients power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rWilden"/>
    <tag group="ReqClass" tag="clsAssassi"/>
    </thing>
  <thing id="ftGhostWin" name="Ghostly Wind" description="{b}Benefit{/b}: If you spend your second wind while your shadow jaunt power remains unexpended, you gain insubstantial until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shadar-kai, shadow jaunt racial power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShadarkai"/>
    <prereq message="&apos;Shadow jaunt&apos; feature required">
      <validate>perform hero.childfound[pRacShaJau].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftGhostSty" name="Ghostwalker Style" description="{b}Benefit{/b}: You gain a benefit with any of the following powers you possess.{br}Deft Strike (rogue): If you didn&apos;t move before the attack, you can shift 1 square as a free action after the attack.{br}Nimble Strike (ranger): While using this exploit, you can move 2 squares before or after the attack, instead of shifting 1 square." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class, trained in Stealth"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    <tag group="ReqSkill" tag="skStealth"/>
    <eval phase="Setup" priority="1000"><![CDATA[
      #thingdescappend[pRogDefStr, "{b}Addition by Ghostwalker Style{/b}: If you didn't move before the attack, you can shift 1 square as a free action after the attack."]
      #thingdescappend[pRgrNimStr, "{b}Addition by Ghostwalker Style{/b}: While using this exploit, you can move 2 squares before or after the attack, instead of shifting 1 square."]]]></eval>
    </thing>
  <thing id="ftGhostHer" name="Ghostwise Heritage" description="{b}Benefit{/b}: After meditating for one minute, choose one ally you can see. Whenever that ally is within 10 squares of you, any time you are both required to make a Perception check, both of you can use the higher result of your two rolls.{br}By meditating again, you can change the ally to whom you are linked." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Halfling"/>
    <usesource source="FRPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfling"/>
    </thing>
  <thing id="ftGiantKil" name="Giant Killer" description="{b}Benefit{/b}: When you score a critical hit against a creature of Large or greater size, you deal an extra 1d6 damage." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftGiftEte" name="Gift of the Eternal" description="{b}Benefit{/b}: At the end of your first turn in each encounter, you regain 1 power point." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Elan Heritage, Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="ElanBloodl" name="Elan Bloodline"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="PowerUse" tag="Encounter"/>
    <exprreq message="Feat &apos;Elan Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftElanHer] <> 0]]></exprreq>
    </thing>
  <thing id="ftGiftTre" name="Gift of the Trees" description="{b}Benefit{/b}: When you use a wilden racial power, one ally within 10 squares of you can shift 1 square as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wilden, Nature&apos;s Aspect racial trait"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rWilden"/>
    </thing>
  <thing id="ftGifteDea" name="Gifted Death Dealer" description="{b}Benefit{/b}: When you hit a creature with your eldritch blast, you deal a feat bonus to damage equal to your Intelligence modifier." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Eladrin, warlock, eldritch blast power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rEladrin"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    </thing>
  <thing id="ftGithzBea" name="Githzerai Beast Mastery" description="{b}Benefit{/b}: Your beast companion gains a +2 bonus to saving throws against the dazed, dominated, and stunned conditions. When you use your second wind, your beast companion can shift 3 squares as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Githzerai, Beast Mastery class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    </thing>
  <thing id="ftGithzBla" name="Githzerai Blade Master" description="{b}Benefit{/b}: You gain proficiency with all military heavy blades, as well as the bastard sword and the fullblade. Additionally, you gain a +2 feat bonus to damage rolls with these weapons. This bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Githzerai"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform hero.assign[WpGrpRqMil.wgHeavyBl]
      perform hero.assign[WeaponProf.wpBastaswo]
      perform hero.assign[WeaponProf.wpFullblad]
      var bonus as number
      if (#level[] < 11) then
        bonus = 2
      elseif (#level[] < 21) then
        bonus = 3
      else
        bonus = 4
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgHeavyBl | WeaponProf.wpBastaswo | WeaponProf.wpFullblad"
        perform eachpick.field[wpDamFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftGithzHea" name="Githzerai Healer" description="{b}Benefit{/b}: Whenever you use a healing power, each target of that power can make a saving throw with a +2 bonus against an effect that includes dazed, dominated, or stunned." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, githzerai"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    </thing>
  <thing id="ftGithzMob" name="Githzerai Mobility" description="{b}Benefit{/b}: You gain a bonus to all defenses against opportunity attacks equal to 1 + your Wisdom modifier." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Wis 13, githzerai"/>
    <fieldval field="reqWis" value="13"/>
    <usesource source="Dragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    </thing>
  <thing id="ftGithzPla" name="Githzerai Planewalker" description="{b}Benefit{/b}: When you perform the ritual Linked Portal, Planar Portal, or True Portal, you halve the time required to perform it and gain a +5 bonus to any required skill check. In addition, you can master those rituals even if you don&apos;t have the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, githzerai"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    </thing>
  <thing id="ftGlasyCha" name="Glasya&apos;s Charming Words" description="{b}Benefit{/b}: You gain a +1 feat bonus to attack rolls with powers that have the charm keyword.{br}{br}When you score a critical hit with a charm power, the target is also dominated until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, tiefling"/>
    <usesource source="PHRcTief"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      foreach pick in hero from Power where "EffectType.Charm"
        perform eachpick.field[pwAtkFeat].modify[+,1,""]
        nexteach]]>
      <before name="Power attack final"/>
      </eval>
    </thing>
  <thing id="ftGlasyReb" name="Glasya&apos;s Rebuke" description="{b}Benefit{/b}: Each time a target takes damage from hellish rebuke because you took damage, one enemy within 5 squares of the target also takes 5 fire damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, Infernal Pact, hellish rebuke power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <prereq message="&apos;Infernal Pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkInfPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftGlidiBea" name="Gliding Beast" description="{b}Benefit{/b}: After your beast companion hits with a melee basic attack, it can shift 1 square as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, ranger, Beast Mastery class feature (cat or raptor)"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsRanger"/>
    </thing>
  <thing id="ftGlittGam" name="Glittergold&apos;s Gambit" description="{b}Benefit{/b}: You can invoke the power of your deity to use Glittergold&apos;s gambit." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Garl Glittergold"/>
    <usesource source="FRPG"/>
    <tag group="ftDescript" tag="Divinity"/>
