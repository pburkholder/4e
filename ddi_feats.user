  <thing id="ftBattlTra" name="Battle Trample" description="{b}Benefit{/b}: You gain the battle trample power." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtBattTra"></bootstrap>
    </thing>
  <thing id="ftBattlVet" name="Battle-Scarred Veteran" description="{b}Benefit{/b}: You gain a benefit with any of the following exploits you possess." compset="Feat" uniqueness="useronce">
