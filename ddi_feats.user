    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqRace" tag="rDragonbor"/>
    <exprreq message="Feat &apos;Burning Breath&apos; required"><![CDATA[hero.tagis[Feat.ftBurniBre] <> 0]]></exprreq>
    </thing>
  <thing id="ftConfoLuc" name="Confounding Luck" description="{b}Benefit{/b}: When you use your second chance racial power and the triggering enemy&apos;s attack misses, that enemy takes a -2 penalty to attack rolls until the end of your next turn." compset="Feat" uniqueness="useronce">
