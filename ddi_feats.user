    <exprreq message="Feat &apos;Two-Weapon Fighting&apos; required"><![CDATA[hero.tagis[Feat.ftTwoWeFig] <> 0]]></exprreq>
    </thing>
  <thing id="ftTwoWeThr" name="Two-Weapon Threat" description="{b}Benefit{/b}: While you are wielding two melee weapons, you gain a +3 bonus to damage rolls with opportunity attacks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dex 13, Two-Weapon Fighting feat"/>
    <fieldval field="reqDex" value="13"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <exprreq message="Feat &apos;Two-Weapon Fighting&apos; required"><![CDATA[hero.tagis[Feat.ftTwoWeFig] <> 0]]></exprreq>
    </thing>
  <thing id="ftTwofoCur" name="Twofold Curse" description="{b}Benefit{/b}: When you use your Warlock&apos;s Curse class feature, you can curse the two nearest enemies." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Warlock, Warlock&apos;s Curse class feature"/>
