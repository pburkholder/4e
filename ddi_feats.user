  <thing id="ftRecklCha" name="Reckless Charge" description="{b}Benefit{/b}: When you make a charge attack, you can take a -2 penalty to AC until the start of your next turn to gain a +1 bonus to the attack rolls of that attack." compset="Feat" uniqueness="useronce">
