  <thing id="ftRazorSha" name="Razor Shard Swarm" description="{b}Benefit{/b}: When you use shard swarm, each enemy in the burst takes damage equal to your Intelligence modifier, your Wisdom modifier, or your Charisma modifier." compset="Feat" uniqueness="useronce">
