  <thing id="ftSickeFin" name="Sickening Finish" description="{b}Benefit{/b}: Whenever you reduce an enemy to 0 hit points, you gain a +1 feat bonus to all defenses until the end of your next turn." compset="Feat" uniqueness="useronce">
