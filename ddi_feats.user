    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsDruid"/>
    </thing>
  <thing id="ftQuickWil" name="Quick Wild Shape" description="{b}Benefit{/b}: You can use your wild shape power as a free action during your turn." compset="Feat" uniqueness="useronce">
