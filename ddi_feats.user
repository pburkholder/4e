  <thing id="ftHawkeWar" name="Hawkeye Warrior" description="{b}Benefit{/b}: When you use the elven accuracy racial power, you gain a +1 bonus to attack rolls against the target of the attack until the end of the encounter." compset="Feat" uniqueness="useronce">
