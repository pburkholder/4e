    <tag group="Tier" tag="Heroic"/>
    <exprreq message="Feat &apos;Dusk Elf Stealth&apos; required"><![CDATA[hero.tagis[Feat.ftDuskElf] <> 0]]></exprreq>
    </thing>
  <thing id="ftGazeRui" name="Gaze of Ruin" description="{b}Benefit{/b}: You gain gaze of ruin as an encounter power. In addition, you gain a +2 bonus to Insight and Perception checks made against other tieflings." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="TieflingBl" name="Tiefling Bloodline"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtGazeRui"></bootstrap>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skInsight,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftGenasFir" name="Genasi Fire Affinity" description="{b}Benefit{/b}: If you have resistance to fire, increase that resistance by 2. Whenever a creature makes a saving throw to end ongoing fire damage from one of your powers, that creature gets -1 penalty to the roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Genasi"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGenasi"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[rsFire,rsExtraPre,2,""]]]></eval>
    </thing>
  <thing id="ftGenasFro" name="Genasi Frost Affinity" description="{b}Benefit{/b}: If you have resistance to cold, increase that resistance by 3. You are immune to any slow and immobilize effects caused by cold powers." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Genasi"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGenasi"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[rsCold,rsExtraPre,2,""]]]></eval>
    </thing>
  <thing id="ftGestaAnc" name="Gestalt Anchor" description="{b}Benefit{/b}: You and allies within the radius of your Ardent Mantle gain a +2 bonus to initiative checks.{br}The first time you drop to 0 power points during an encounter, allies within the radius of your Ardent Mantle gain a +2 bonus to all defenses until the end of your next turn." compset="Feat" uniqueness="useronce">
