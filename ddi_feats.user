    <fieldval field="reqWis" value="15"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="Tier" tag="Paragon"/>
    </thing>
  <thing id="ftSmallVen" name="Small Vengeance" description="{b}Benefit{/b}: You gain the Channel Divinity power small vengeance." compset="Feat" uniqueness="useronce">
