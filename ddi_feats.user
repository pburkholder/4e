    <tag group="ReqRace" tag="rKalashtar"/>
    <tag group="ReqClass" tag="clsArdent"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[trSpeed,trtFeat,2,""]]]>
      <before name="Calc trtFinal"/>
      </eval>
    <prereq message="&apos;Ardent Mantle&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fArdArdMan"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftGetin" name="Get in Your Head" description="{b}Benefit{/b}: Your fade away power gains the augmentable and teleportation keywords. When you would use fade away, you can spend 1 power point to also teleport 3 squares as a free action after you become invisible." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Gnome, any psionic class, fade away power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGnome"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    </thing>
  <thing id="ftGhostEye" name="Ghost Eyes" description="{b}Benefit{/b}: You gain a +2 bonus to attack rolls against invisible creatures. In addition, you gain a +5 feat bonus to Perception checks actively made to find creatures that are hidden from you." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wisdom 13"/>
    <fieldval field="reqWis" value="13"/>
    <usesource source="PlEsShadow"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftGhostFei" name="Ghost Feint" description="{b}Benefit{/b}: When you use ghost sound, you gain a +5 power bonus to your next Bluff check before the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Gnome, ghost sound power, trained in Bluff"/>
