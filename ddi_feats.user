    <tag group="Multiclass" tag="WepMastery"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqSkill" tag="skArcana"/>
    <bootstrap thing="fArdPsiAug"></bootstrap>
    <eval phase="Traits" priority="1000">#trkmax[trkPower] += 1</eval>
    </thing>
  <thing id="ftAxeExp" name="Axe Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls you make with an axe. This bonus increases to +2 at 11th level and +3 at 21st level.{br}Also, when rolling damage for a weapon attack you make with an axe, you can reroll one damage die that results in a 1, but you must use the second result." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsHeroFa"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgAxe"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftAxeMas" name="Axe Mastery" description="{b}Benefit{/b}: When you make a melee weapon attack with an axe, you can score a critical hit on a natural roll of 19 or 20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Str 21, Con 17"/>
    <fieldval field="reqStr" value="21"/>
    <fieldval field="reqCon" value="17"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    </thing>
  <thing id="ftAxeStr" name="Axe Strike" description="{b}Benefit{/b}: When you miss a target with a weapon attack using an axe, you can expend a use of power strike to deal 5 damage to the target, even if it is a minion. This damage increases to 10 at 11th level and 15 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Power strike power"/>
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftBaalzDes" name="Baalzebul&apos;s Desperate Gambit" description="{b}Benefit{/b}: When you first become bloodied in an encounter, you can roll a saving throw, with a bonus equal to the number of enemies adjacent to you. If you succeed, spend a healing surge. If you fail, you lose a healing surge." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, tiefling"/>
    <usesource source="PHRcTief"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="PowerUse" tag="Encounter"/>
    </thing>
  <thing id="ftBackto" name="Back to the Wall" description="{b}Benefit{/b}: Whenever you are adjacent to a wall, you gain a +1 bonus to melee attack rolls, melee damage rolls, and AC." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftBackbSta" name="Backbreaking Staff" description="{b}Benefit{/b}: When you use backstab on an attack using a staff, on a hit the target is also dazed until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Backstab"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftBackstab" name="Backstabber" description="{b}Benefit{/b}: The extra damage dice from your Sneak Attack class feature increase from d6s to d8s." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Rogue, Sneak Attack class feature"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRogue"/>
    <prereq message="&apos;Sneak Attack&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fRogSneAtt"
call ReqFeature

featureid = "fThfSneAtt" 
call ReqFeature

featureid = "fRogHSnAtt" 
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftBaelTur" name="Bael Turath Born" description="{b}Benefit{/b}: Your fire resistance improves; you now have resist fire 8 + your level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, tiefling, warlock"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~resist Fire 8 + level
      var bonus as number
      bonus = 8 + hero.tagvalue[Level.?]
      #traitmodify[rsFire,rsTotal,bonus,""]]]></eval>
    </thing>
  <thing id="ftBalefMal" name="Baleful Malediction" description="{b}Benefit{/b}: Whenever you use a divine encounter or daily attack power on your turn, each target hit by the power takes a -2 penalty to attack rolls against you until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Invoker, Covenant of Malediction class feature"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    <prereq message="&apos;Covenant of Malediction&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fInvCovMal"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftBalicHig" name="Balican High Praetor" description="{b}Benefit{/b}: Whenever an enemy you have cursed drops to 0 hit points, you gain the following benefit in addition to regaining the use of your fell might. The next ally you can see that hits with a damaging attack before the start of your next turn subjects its target to your Warlock&apos;s Curse. If the target is already under your curse, the ally&apos;s attack instead deals extra damage equal to 3 + your Intelligence modifier.{br}You can augment powers associated with this feat with your fell might. When you hit an enemy with an augmented power associated with this feat, each ally adjacent to the target can either shift 1 square as a free action or gain a +2 power bonus to all defenses until the start of your next turn.{br}Finally, you can use Charisma instead of Constitution for attack rolls and damage rolls with any power associated with this feat." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, Sorcerer-King Pact class feature, Balican Praetor feat"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="GreaterPac" name="Greater Pact"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <exprreq message="Feat &apos;Balican Praetor&apos; required"><![CDATA[hero.tagis[Feat.ftBalicPra] <> 0]]></exprreq>
    <prereq message="&apos;Sorcerer-King Pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkSorPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftBalicPra" name="Balican Praetor" description="{b}Benefit{/b}: You gain a +2 feat bonus to Diplomacy checks.{br}When you augment hand of blight, you can forgo the extra damage the augment would deal to instead grant the target vulnerability to all damage equal to your Intelligence modifier until the start of your next turn. Each time the target takes damage while it has this vulnerability, reduce the vulnerability by 1 (to a minimum of 0)." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, Sorcerer-King Pact class feature"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="LesserPact" name="Lesser Pact"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skDiplomac,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    <prereq message="&apos;Sorcerer-King Pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkSorPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftBalinPre" name="Balinor&apos;s Prey" description="{b}Benefit{/b}: You gain the Channel Divinity power Balinor&apos;s prey." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Balinor"/>
