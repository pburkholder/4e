    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform hero.assign[WeaponProf.wpBlowgun]
      foreach pick in hero from WeaponBase where "thingid.wpBlowgun"
        eachpick.field[wpShort].value = 10
        eachpick.field[wpLong].value = 20
        perform eachpick.assign[WepProp.HighCrit]
        perform eachpick.delete[WepProp.LoadMinor]
        perform eachpick.assign[WepProp.LoadFree]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftBludgExp" name="Bludgeon Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls you make with a hammer or a mace. This bonus increases to +2 at 11th level and +3 at 21st level.{br}You also gain a +1 feat bonus to the number of squares you push or slide creatures with weapon attacks you make with a hammer or a mace." compset="Feat" uniqueness="useronce">
