      if (compare(field[usrSelect].text, "Lightning") <> 0) then
        resid = "thingid.rs" & field[usrSelect].text
      else
        resid = "thingid.rsLightn"
        endif
      perform hero.findchild[Resistance,resid].field[rsTotal].modify[+,5,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftSoulsMoi" name="Soulstealer of Moil" description="{b}Benefit{/b}: Whenever you use a shadow summoning power, you gain 5 temporary hit points. These temporary hit points increase to 10 at 11th level and 15 at 21st level." compset="Feat" uniqueness="useronce">
