    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgHeavyBl"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftHeavyBla" name="Heavy Blade Mastery" description="{b}Benefit{/b}: When you make a melee weapon attack with a heavy blade, you can score a critical hit on a natural roll of 19 or 20." compset="Feat" uniqueness="useronce">
