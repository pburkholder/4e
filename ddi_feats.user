  <thing id="ftPhantEli" name="Phantasmal Elixir" description="{b}Benefit{/b}: You can use your fade away power when you or an ally affected by your shielding elixir power takes damage. If the ally triggered the power, both you and the ally become invisible until you attack or until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Gnome, fade away racial power, artificer, shielding elixir power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGnome"/>
    <tag group="ReqClass" tag="clsArtific"/>
    <prereq message="&apos;Fade away&apos; feature required">
      <validate>perform hero.childfound[pRacFadAwa].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPhantBla" name="Phantom Blade Evasion" description="{b}Benefit{/b}: When you use your second chance racial power, you can use shadow step as a free action when the attack is resolved." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Halfling, assassin, shadow step power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfling"/>
    <tag group="ReqClass" tag="clsAssassi"/>
    </thing>
  <thing id="ftPhantEch" name="Phantom Echoes" description="{b}Benefit{/b}: When you use an arcane illusion power and hit a target, you gain combat advantage against that target until the end of your next turn. If the illusion power has an effect that a save can end, you instead gain combat advantage against the target until it saves against that effect." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Cha 13, wizard"/>
    <fieldval field="reqCha" value="13"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWizard"/>
    </thing>
  <thing id="ftPickExp" name="Pick Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls that you make with a pick. This bonus increases to +2 at 11th level and +3 at 21st level.{br}In addition, you gain a +1 bonus to pick damage rolls against any enemy that has a size category larger than yours. This bonus increases to +2 at 11th level and +3 at 21st level." compset="Feat" uniqueness="useronce">
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgPick"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftPickMas" name="Pick Mastery" description="{b}Benefit{/b}: When you make a melee weapon attack with a pick, you can score a critical hit on a natural roll of 19 or 20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Str 21, Con 17"/>
    <fieldval field="reqStr" value="21"/>
    <fieldval field="reqCon" value="17"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    </thing>
  <thing id="ftPickStr" name="Pick Strike" description="{b}Benefit{/b}: When you use power strike with a pick, you deal the maximum damage you could roll for power strike if your attack roll against the target was 18-20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Power strike power"/>
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPiercPal" name="Piercing Palm" description="{b}Benefit{/b}: You can deal your Sneak Attack damage with your monk unarmed strike. You can use your monk unarmed strike with melee or close rogue powers and rogue paragon path powers that require the use of a light blade." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Monk, Rogue, Sneak Attack class feature, Unarmed Combatant class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsMonk"/>
    <tag group="ReqClass" tag="clsRogue"/>
    <prereq message="&apos;Sneak Attack&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fRogSneAtt"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPiercPic" name="Piercing Pick" description="{b}Benefit{/b}: Whenever you use a pick to make a melee basic attack, you can target AC or Fortitude." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, any martial class"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    </thing>
  <thing id="ftPillaHea" name="Pillar of Heaven" description="{b}Benefit{/b}: When you miss each target with an invoker encounter attack power while using a staff, you gain a +2 bonus to all defenses until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, invoker"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    </thing>
  <thing id="ftPinDow" name="Pin Down" description="{b}Benefit{/b}: A prone target you are grabbing can&apos;t stand until you end the grab or until it escapes." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 17, fighter"/>
    <fieldval field="reqStr" value="17"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftPinniCha" name="Pinning Challenge" description="{b}Benefit{/b}: Whenever you hit a target already marked by you with a two-handed weapon and when using a basic attack, the target is also immobilized until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, fighter, Fighter Weapon Talent class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftPiousCha" name="Pious Champion" description="{b}Benefit{/b}: When you use your call of virtue or lay on hands, you can target one or two creatures. If you target two creatures with lay on hands, you spend one healing surge and each target regains hit points as normal for the power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, paladin, call of virtue or lay on hands power"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsPaladin"/>
    </thing>
  <thing id="ftPlaguCha" name="Plague of Chance" description="{b}Benefit{/b}: When you use second chance and the rerolled attack roll misses, the attacker takes fire and necrotic damage equal to your level and cannot see you until the end of your next turn. If you have the Student of the Plague feat, the attacker also takes fire and necrotic damage equal to your level if the rerolled attack roll hits." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Halfling, second chance racial power, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfling"/>
    <prereq message="&apos;Second chance&apos; feature required">
      <validate>perform hero.childfound[pRacSecCha].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPlaguBre" name="Plagued Breath" description="{b}Benefit{/b}: Your dragon breath deals necrotic damage in addition to the damage type it already deals. Also, while you are bloodied, your breath attack deals 2 extra damage. If you have the Student of the Plague feat, a creature you hit with dragon breath also takes ongoing 5 necrotic damage (save ends)." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dragonborn, dragon breath racial power, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDragonbor"/>
    <eval phase="Traits" priority="1000">doneif (activated = 0)

      perform hero.child[pRacDraBre].assign[DamageType.Necrotic]</eval>
    <prereq message="&apos;Dragon breath&apos; feature required">
      <validate>perform hero.childfound[pRacDraBre].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPlaguReg" name="Plagued Regeneration" description="{b}Benefit{/b}: Increase the regeneration longtooth shifting grants you by 1. At 11th level, increase it by 2, and at 21st level, increase it by 3. Whenever you regain hit points from this regeneration, nonminion creatures adjacent to you take necrotic damage equal to the hit points you regained. If you have the Student of the Plague feat, any ally adjacent to you takes only 1 point of necrotic damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Longtooth shifter, longtooth shifting racial power, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShifteLon"/>
    <prereq message="&apos;Longtooth shifting&apos; feature required">
      <validate>perform hero.childfound[pRacLonShi].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPlaguAcc" name="Plagueforged Accuracy" description="{b}Benefit{/b}: Whenever you use elven accuracy, the rerolled attack roll ignores cover and concealment, and it deals fire and necrotic damage in addition to its normal damage types if it hits. If you have the Student of the Plague feat, use elven accuracy, and hit with the rerolled attack roll, the target of that roll and each creature adjacent to it take fire and necrotic damage equal to 1 + one-half your level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Elf, elven accuracy racial power, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rElf"/>
    <prereq message="&apos;Elven accuracy&apos; feature required">
      <validate>perform hero.childfound[pRacElvAcc].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPlaguBlo" name="Plaguing Bloodhunt" description="{b}Benefit{/b}: When you hit a bloodied enemy, it takes 3 extra necrotic damage. Increase this damage to 6 at 11th level, and 9 at 21st level. If you have the Student of the Plague feat, the extra damage becomes fire and necrotic damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling, Bloodhunt racial trait, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    </thing>
  <thing id="ftPlanaBlu" name="Planar Blurring" description="{b}Benefit{/b}: Before or after you use fey step, you can slide one creature adjacent to you 1 square. If you have the Student of the Plague feat, before or after you use fey step you can also teleport a creature adjacent to you 2 squares." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Eladrin, fey step racial power, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rEladrin"/>
    <prereq message="&apos;Fey step&apos; feature required">
      <validate>perform hero.childfound[pRacFeySte].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPlantSap" name="Plant&apos;s Sap" description="{b}Benefit{/b}: You do not need to eat. You also do not need to sleep and instead enter a state of inactivity for 4 hours to gain the benefits of an extended rest. While in this state, you are fully aware of your surroundings and notice approaching enemies and other events as normal.{br}In addition, you gain a +2 feat bonus to saving throws against ongoing damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wilden"/>
    <usesource source="HeroFeywil"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rWilden"/>
    </thing>
  <thing id="ftPlatiRev" name="Platinum Revelation" description="{b}Benefit{/b}: You gain training in Insight. You gain the Bahamut&apos;s shield power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 13, you must worship Bahamut"/>
    <fieldval field="reqStr" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Multiclass" tag="WepMastery"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtBahaShi"></bootstrap>
    <eval phase="Initialize" priority="1000">perform hero.child[skInsight].assign[Helper.TrainedAut]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftPledgRet" name="Pledge of Retribution" description="{b}Benefit{/b}: If you miss your oath of enmity target with an attack, you gain a +1 bonus to the next attack roll you make against that target before the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, avenger, Censure of Retribution class feature, oath of enmity power"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsAvenger"/>
    <prereq message="&apos;Censure of Retribution&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fAveCenRet"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPlungBla" name="Plunging Blade" description="{b}Benefit{/b}: When you make an opportunity attack with a light blade, you score a critical hit on a natural roll of 19 or 20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, fighter"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftPointSho" name="Point-Blank Shot" description="{b}Benefit{/b}: If you make a ranged attack against a foe within 5 squares of you, your attack ignores cover and concealment, including superior cover, but not total concealment." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPointSte" name="Pointed Step Style" description="{b}Benefit{/b}: When your flurry of blows power is triggered by an attack that you make with a spear, one target of that power can be up to 2 squares away from you." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Monk, Flurry of Blows class feature"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsMonk"/>
    </thing>
  <thing id="ftPoisoAda" name="Poison Adaptation" description="{b}Benefit{/b}: You gain resist 5 poison. The resistance increases to 10 at 11th level and 15 at 21st level." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] < 11) then
        bonus = 5
      elseif (#level[] < 21) then
        bonus = 10
      else
        bonus = 15
        endif
      #traitmodify[rsPoison,rsTotal,bonus,""]]]></eval>
    </thing>
  <thing id="ftPoisoInu" name="Poison Inured" description="{b}Benefit{/b}: You gain a feat bonus equal to your Constitution modifier to saving throws against poison effects." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Con 13"/>
    <fieldval field="reqCon" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPoisoPla" name="Poison Plague Feast" description="{b}Benefit{/b}: Whenever you save against a poison effect, you gain a number of temporary hit points equal to 1 + one-half your level. If you have the Student of the Plague feat, whenever you save against a poison effect, you also gain a +2 bonus to saving throws until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dwarf, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDwarf"/>
    </thing>
  <thing id="ftPoisoPar" name="Poison&apos;s Partner" description="{b}Benefit{/b}: An enemy suffering from a poison effect grants combat advantage to you." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    </thing>
  <thing id="ftPoisoner" name="Poisoner" description="{b}Benefit{/b}: You gain training in the Thievery skill. Your attacks that have a poison effect ignore the first 5 points of poison resistance. This increases to 10 points at 11th level, and again to 15 points at 21st level. You treat a creature that is immune to poison as if it had resist poison 20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Training in Dungeoneering or Nature"/>
    <usesource source="Dragon"/>
    <tag group="Multiclass" tag="WepMastery"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Initialize" priority="1000">perform hero.child[skThievery].assign[Helper.TrainedAut]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftPoisoExp" name="Poisoner Expert" description="{b}Benefit{/b}: You can swap one 6th-level or higher utility power you know for the potent poison utility power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="8th level, Poisoner"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="Multiclass"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="8"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtPotePoi"></bootstrap>
    <exprreq message="Feat &apos;Poisoner&apos; required"><![CDATA[hero.tagis[Feat.ftPoisoner] <> 0]]></exprreq>
    </thing>
  <thing id="ftPoisoNov" name="Poisoner Novice" description="{b}Benefit{/b}: You can swap one 3rd-level or higher encounter attack power you know for the improvised poison attack power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="4th level, Poisoner"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="Multiclass"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="4"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtImprPoi"></bootstrap>
    <exprreq message="Feat &apos;Poisoner&apos; required"><![CDATA[hero.tagis[Feat.ftPoisoner] <> 0]]></exprreq>
    </thing>
  <thing id="ftPoisoSpe" name="Poisoner Specialist" description="{b}Benefit{/b}: You can swap one 9th-level or higher daily attack power you know for the progressive toxin attack power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="10th level, Poisoner"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="Multiclass"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="10"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtProgTox"></bootstrap>
    <exprreq message="Feat &apos;Poisoner&apos; required"><![CDATA[hero.tagis[Feat.ftPoisoner] <> 0]]></exprreq>
    </thing>
  <thing id="ftPoisoBea" name="Poisonous Beast" description="{b}Benefit{/b}: Whenever your beast companion hits a creature with a melee basic attack, that creature takes ongoing 5 poison damage (save ends).{br}This ongoing damage increases to 10 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, ranger, Beast Mastery class feature (serpent or spider)"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsRanger"/>
    </thing>
  <thing id="ftPoleaExp" name="Polearm Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls that you make with a polearm. This bonus increases to +2 at 11th level and +3 at 21st level.{br}In addition, while you hold a two-handed polearm in both hands, you gain a +2 bonus to all defenses against charge attacks." compset="Feat" uniqueness="useronce">
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgPolearm"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftPoleaFla" name="Polearm Flanker" description="{b}Benefit{/b}: When you make a melee attack with a polearm against a target that is not adjacent to you, choose an unoccupied square that is adjacent to both you and the target. You are considered to occupy that square for the purpose of determining if you flank the target for the attack." compset="Feat" uniqueness="useronce">
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPoleaGam" name="Polearm Gamble" description="{b}Benefit{/b}: When a nonadjacent enemy enters a square adjacent to you, you can make an opportunity attack with a polearm against that enemy, but you grant combat advantage to that enemy until the end of the enemy&apos;s turn.{br}{b}Special{/b}: Weapon feat" compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Str 15, Wis 15"/>
    <fieldval field="reqStr" value="15"/>
    <fieldval field="reqWis" value="15"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPoleaMom" name="Polearm Momentum" description="{b}Benefit{/b}: Whenever you use a polearm or a spear attack to push or slide a target 2 or more squares, you can also knock that target prone at the end of the forced movement." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dex 15, Wis 15, fighter"/>
    <fieldval field="reqDex" value="15"/>
    <fieldval field="reqWis" value="15"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftPotenCha" name="Potent Challenge" description="{b}Benefit{/b}: If you hit a foe with an attack granted by your Combat Challenge class feature, add your Constitution modifier to the damage roll.{br}{b}Special{/b}: This benefit applies only to attacks made with two-handed weapons." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Con 15, fighter, Combat Challenge class feature"/>
    <fieldval field="reqCon" value="15"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsFighter"/>
    <prereq message="&apos;Combat Challenge&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fFtrComCha"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPotenReb" name="Potent Rebirth" description="{b}Benefit{/b}: When you drop to 0 hit points or fewer, you gain a +2 bonus to attack rolls and damage rolls until the end of the encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Deva"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDeva"/>
    </thing>
  <thing id="ftPotenRes" name="Potent Restorables" description="{b}Benefit{/b}: Your artificer healing powers restore 2 extra hit points. The extra hit points increase to 3 at 6th level, to 4 at 11th level, to 5 at 16th level, to 6 at 21st level, and to 7 at 26th level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Artificer"/>
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsArtific"/>
    </thing>
  <thing id="ftPouncCat" name="Pouncing Cat" description="{b}Benefit{/b}: Your beast companion gains a +2 bonus to saving throws.{br}While your beast companion acts independently, it need not move adjacent to you but must remain within 10 squares of you.{br}In addition, if it hits a creature with a charge attack, it can make an additional melee basic attack against that creature as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ranger, Beast Mastery class feature (cat)"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    </thing>
  <thing id="ftPouncFor" name="Pouncing Form" description="{b}Benefit{/b}: When you use wild shape to change from your humanoid form to beast form, you can shift a number of squares equal to your Dexterity modifier." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, druid, Primal Predator class feature, wild shape power"/>
    <usesource source="Primal"/>
