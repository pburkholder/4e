    <exprreq message="Feat &apos;Forgeborn Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftForgeHer] <> 0]]></exprreq>
    </thing>
  <thing id="ftAnchoPus" name="Anchoring Push" description="{b}Benefit{/b}: When you use your forceful push, you also slow the target until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, psion, forceful push power"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftAncieCov" name="Ancient Covenant of Nessus" description="{b}Benefit{/b}: When you use a divine encounter or daily attack power on your turn, instead of your normal covenant manifestation, you can allow one target of the power to make a basic attack as a free action. If the target makes this attack, it takes fire damage equal to 5 + your Intelligence modifier after resolving the attack.{br}{br}The damage increases to 10 + your Intelligence modifier at 11th level and 15 + your Intelligence modifier at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling, invoker, Divine Covenant class feature"/>
    <usesource source="PHRcTief"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    <prereq message="&apos;Divine Covenant&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fInvDivCov"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftAncieLor" name="Ancient Lore of the Dawn War" description="{b}Benefit{/b}: You have studied the lore of old deities and the history of the Dawn War. You gain a +2 feat bonus to History and Religion checks, and you gain Supernal as a language." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftAncieSou" name="Ancient Soul" description="{b}Benefit{/b}: Your dragon breath power is considered an arcane attack power.{br}In addition, if your dragon breath power deals damage of the same type as your Dragon Magic class feature gives you resistance to, you gain the following benefit:{br}Whenever you take damage of the type dealt by your dragon breath (after the damage dealt is reduced by your resistance), you regain the use of your dragon breath if you have already expended it in this encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dragonborn, Dragon Magic sorcerer, dragon breath racial power"/>
    <usesource source="PHRcDragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDragonbor"/>
    <eval phase="Traits" priority="1000">perform hero.childfound[pRacDraBre].assign[PowerSrc.Arcane]</eval>
    <prereq message="&apos;Dragon breath&apos; feature required">
      <validate>perform hero.childfound[pRacDraBre].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftAncieSto" name="Ancient Stone" description="{b}Benefit{/b}: When your stone&apos;s endurance racial power ends, you gain resist 10 to all damage until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, goliath"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqRace" tag="rGoliath"/>
    </thing>
  <thing id="ftAncieWis" name="Ancient Wisdom" description="{b}Benefit{/b}: When you use voyage of the ancients, the enemy you choose also grants combat advantage to each ally adjacent to your spirit companion until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wilden, shaman, voyage of the ancients power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rWilden"/>
    <tag group="ReqClass" tag="clsShaman"/>
    </thing>
  <thing id="ftAngelHar" name="Angelic Harrier" description="{b}Benefit{/b}: When you hit only one target with an invoker ranged encounter attack or an invoker ranged daily attack, you can slide the target 1 square after the power&apos;s effect is resolved." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Invoker"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    </thing>
  <thing id="ftAngelPro" name="Angelic Protection" description="{b}Benefit{/b}: When you use lay on hands, you can instead grant the target temporary hit points equal to the number of hit points it would have regained." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, paladin, lay on hands"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPaladin"/>
    <prereq message="&apos;Lay on hands&apos; feature required">
      <validate>perform hero.childfound[pPalLayOn].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftAngerUnl" name="Anger Unleashed" description="{b}Benefit{/b}: The first time you are bloodied during an encounter, you gain a +2 bonus to attack rolls until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Half-orc"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfOrc"/>
    </thing>
  <thing id="ftAnghaFav" name="Angharradh&apos;s Favor" description="{b}Benefit{/b}: You can invoke the power of your deity to use Angharradh&apos;s favor." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Angharradh"/>
    <usesource source="FRPG"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="ReqDeity" tag="dtyAngharr"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtAnghFav"></bootstrap>
    </thing>
  <thing id="ftAngryGra" name="Angry Grandfather" description="{b}Benefit{/b}: You gain a bonus to death saving throws equal to the number of rages you have expended since your last extended rest." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Barbarian"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqClass" tag="clsBarbari"/>
    </thing>
  <thing id="ftAnimaEmp" name="Animal Empathy" description="{b}Benefit{/b}: You gain a +2 feat bonus to Nature checks. Also, when you would make an Insight check against a natural beast, you can make a Nature check instead." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Trained in Nature"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqSkill" tag="skNature"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skNature,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftAnkleBit" name="Ankle Biter" description="{b}Benefit{/b}: You gain a +1 feat bonus to damage rolls against creatures larger than you. This bonus increases to +2 at 11th level and to +3 at 21st level.{br}Also, when you score a critical hit against a creature larger than you, the creature takes 1d6 extra damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Goblin race"/>
    <usesource source="IntoUnknow"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftAnkleCut" name="Ankle Cutter" description="{b}Benefit{/b}: When you hit a Large or larger enemy and would deal Sneak Attack damage against that target, you can forgo rolling Sneak Attack damage and instead cause the target to be slowed until the end of your next turn. Using this option counts as using Sneak Attack for the round." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Halfling, rogue, Sneak Attack class feature"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfling"/>
    <tag group="ReqClass" tag="clsRogue"/>
    <prereq message="&apos;Sneak Attack&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fRogSneAtt"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftAntheCiv" name="Anthem of Civilization" description="{b}Benefit{/b}: You gain the Channel Divinity power anthem of civilization." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship a deity of the civilization domain"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtAnthCiv"></bootstrap>
    </thing>
  <thing id="ftApesRea" name="Ape&apos;s Reach" description="{b}Benefit{/b}: You gain a +2 feat bonus to Athletics checks.{br}While you are in beast form and you are bloodied, you gain +1 reach with melee basic attacks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Druid, wild shape power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsDruid"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skAthletic,trtFeat,2,""]]]></eval>
    </thing>
  <thing id="ftArawaAbu" name="Arawai&apos;s Abundance" description="{b}Benefit{/b}: You gain the Channel Divinity power Arawai&apos;s abundance." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Arawai"/>
    <usesource source="EbPG"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="ReqDeity" tag="dtyArawai"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtArawAbu"></bootstrap>
    </thing>
  <thing id="ftArbitJus" name="Arbiter of Justice" description="{b}Benefit{/b}: You deal +2 damage against bloodied targets. Additionally, whenever an ally bloodies or reduces your oath of enmity target to 0 hit points, you may act as though you had bloodied or reduced the target to 0 hit points." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, avenger, oath of enmity power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsAvenger"/>
    </thing>
  <thing id="ftArcanWar" name="Arcana Ward" description="{b}Benefit{/b}: You gain the Channel Divinity power arcana ward." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship a deity of the arcana domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtArcaWar"></bootstrap>
    </thing>
  <thing id="ftArcanAdm" name="Arcane Admixture" description="{b}Benefit{/b}: Choose one arcane power that you know, and choose acid, cold, fire, lightning, or thunder. The power now deals that damage type in addition to its normal damage type and gains that keyword.{br}Each time you gain a level, you can choose to reassign either the power to which this feat&apos;s effect applies or the additional damage type applied (or both).{br}{b}Special{/b}: You can take this feat more than once. Each time you select this feat, choose a different power. You can&apos;t apply this feat to the same power more than once." compset="Feat">
    <fieldval field="reqText" value="11th level, any arcane class"/>
    <fieldval field="usrCandid1" value="component.Power &amp; PowerSrc.Arcane"/>
    <arrayval field="usrArray" index="0" value="Acid"/>
    <arrayval field="usrArray" index="1" value="Cold"/>
    <arrayval field="usrArray" index="2" value="Fire"/>
    <arrayval field="usrArray" index="3" value="Lightning"/>
    <arrayval field="usrArray" index="4" value="Thunder"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform field[usrChosen1].chosen.assignstr["DamageType." & field[usrSelect].text]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftArcanAeg" name="Arcane Aegis" description="{b}Benefit{/b}: Choose a Swordmage Aegis power. You can use that power once per encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any multiclass swordmage feat, paragon multiclassing as a swordmage"/>
    <usesource source="Arcane"/>
    <tag group="Multiclass" tag="clsSwordma"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Initialize" priority="1000"><![CDATA[
      var title as string
      var expr as string
      title = "Aegis Power"
      expr = "PowerClass.clsSwordma & PowerType.Feature"
      call MultChoice
      doneif (state.isfocus = 0)

      ~change the power to an encounter power
      perform focus.assign[PowerOver.Encounter]]]>
      <before name="opcExpr must be final"/>
      </eval>
    <exprreq message="Requires Swordmage multiclass"><![CDATA[hero.tagis[Multiclass.clsSwordma] <> 0]]></exprreq>
    </thing>
  <thing id="ftArcanFam" name="Arcane Familiar" description="{b}Benefit{/b}: You gain a familiar. For each familiar feat you have beyond this one, your familiar gains a +1 bonus to its defenses." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="ftDescript" tag="Familiar"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftArcanFir" name="Arcane Fire" description="{b}Benefit{/b}: When you hit a target with an arcane fire power, that target gains vulnerable 5 cold against the first arcane attack power you use against it before the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Int 13, any arcane class"/>
    <fieldval field="reqInt" value="13"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftArcanFle" name="Arcane Flexibility" description="{b}Benefit{/b}: When you score a critical hit with a sorcerer power, you can change the damage type dealt by the power to a damage type you have resistance or immunity to." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, sorcerer, Spell Source class feature"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    <prereq message="&apos;Spell Source&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fSorSpeSou"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftArcanFur" name="Arcane Fury" description="{b}Benefit{/b}: When you use your furious assault racial power to deal extra damage to the target of an area or close arcane power, you deal the extra damage to all targets you hit with the power, not just one." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Half-orc, any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfOrc"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftArcanImp" name="Arcane Implement Proficiency" description="{b}Benefit{/b}: Choose a kind of implement associated with any arcane class. You can now use that kind of implement.{br}If that implement is also a weapon, you do not add the weapon&apos;s proficiency bonus to attack rolls made while you wield it as an implement." compset="Feat">
    <fieldval field="reqText" value="Any arcane class"/>
    <fieldval field="usrCandid1" value="thingid.itOrb | thingid.itRod | thingid.itStaff | thingid.itStaff | thingid.itWand | thingid.wgHeavyBl | thingid.wgLightBl | thingid.wpDagger"/>
    <usesource source="Arcane"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      if (field[usrChosen1].ischosen <> 0) then
        perform hero.assignstr["ImplemType." & field[usrChosen1].chosen.idstring]
        endif]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftArcanIni" name="Arcane Initiate" description="{b}Benefit{/b}: You gain training in the Arcana skill.{br}Choose a 1st-level wizard at-will power. You can use that power once per encounter.{br}In addition, you can use wizard implements." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Int 13"/>
    <fieldval field="reqInt" value="13"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="clsWizard"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Initialize" priority="1000"><![CDATA[
      perform hero.child[skArcana].assign[Helper.TrainedAut]

      perform hero.assign[ImplemType.itOrb]
      perform hero.assign[ImplemType.itStaff]
      perform hero.assign[ImplemType.itWand]

      var title as string
      var expr as string
      title = "Bonus Power"
      expr = "PowerClass.clsWizard & PowerType.AtWill"
      call MultChoice
      doneif (state.isfocus = 0)

      ~change it to an encounter power
      perform focus.delete[PowerType.AtWill]
      perform focus.assign[PowerType.Encounter]

      ~ignore any errors about requiring other classes
      perform focus.assign[thing.skipprereq]]]>
      <before name="Calc sklTrained"/>
      <before name="opcExpr must be final"/>
      </eval>
    </thing>
  <thing id="ftArcanLee" name="Arcane Leech" description="{b}Benefit{/b}: When you use an arcane daily attack power with your arcane defiling, each target you hit with the attack cannot regain hit points until the end of your next turn. If you hit at least one target with the attack, you also regain hit points equal to half your healing surge value." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, arcane defiling power"/>
    <usesource source="DarkSun"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    </thing>
  <thing id="ftArcanMal" name="Arcane Malice" description="{b}Benefit{/b}: Whenever you make an arcane daily attack, you gain a +2 bonus to damage rolls against one creature you hit with that attack until the end of the encounter. If you have the Student of the Plague feat, you also gain a +1 bonus to attack rolls against that creature until the end of the encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any arcane class, you have a spellscar"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftArcanMas" name="Arcane Mastery" description="{b}Benefit{/b}: Once per encounter, you can spend an action point to regain the use of a daily wizard power you&apos;ve already used today, instead of taking an extra action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Wizard"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsWizard"/>
    </thing>
  <thing id="ftArcanPor" name="Arcane Porter" description="{b}Benefit{/b}: Your familiar can pick up and carry an unattended object weighing 5 pounds or less. It can hold only one object at a time." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Arcane Familiar feat"/>
    <usesource source="HeroFeywil"/>
    <tag group="ftDescript" tag="Familiar"/>
    <tag group="Tier" tag="Heroic"/>
    <exprreq message="Feat &apos;Arcane Familiar&apos; required"><![CDATA[hero.tagis[Feat.ftArcanFam] <> 0]]></exprreq>
    </thing>
  <thing id="ftArcanPro" name="Arcane Prodigy" description="{b}Benefit{/b}: You gain training in Arcana. Once per encounter as a free action, you can gain a +2 bonus to your next damage roll. The bonus increases to +3 at 11th level and +4 at 21st level.{br}In addition, you can wield sorcerer implements." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Cha 13"/>
    <fieldval field="reqCha" value="13"/>
    <usesource source="PHB2"/>
    <tag group="Multiclass" tag="clsSorcere"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Hide" tag="Special"/>
    <tag group="PowerUse" tag="Encounter"/>
    <bootstrap thing="pFtArcaPro"></bootstrap>
    <eval phase="Traits" priority="1000">perform hero.child[skArcana].assign[Helper.TrainedAut]
      perform hero.assign[ImplemType.itStaff]
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftArcanRea" name="Arcane Reach" description="{b}Benefit{/b}: When using a close arcane attack power, you can choose a square within 2 squares of yours as the origin square. The power still follows the rules for close attacks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Dex 15"/>
    <fieldval field="reqDex" value="15"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="Tier" tag="Paragon"/>
    </thing>
  <thing id="ftArcanRep" name="Arcane Repositioning" description="{b}Benefit{/b}: On your turn, you can use a free action to expend your fey step racial power to teleport one of your conjurations or a creature that you summoned rather than yourself. If you do so, select one conjuration or summoned creature within 5 of squares of you and teleport it 5 squares. You cannot teleport a conjuration larger than 1 square or a summoned creature that is size Large or larger." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Eladrin, artificer"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rEladrin"/>
    <tag group="ReqClass" tag="clsArtific"/>
    </thing>
  <thing id="ftArcanRes" name="Arcane Reserves" description="{b}Benefit{/b}: If all your arcane encounter attack powers are expended, you gain a +2 bonus to damage rolls with your arcane at-will attack powers until the end of the encounter or until you regain the use of one of your arcane encounter attack powers." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Human, any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHuman"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform hero.findchild[Power,"PowerType.Encounter & PowerSrc.Arcane & !Helper.Activated & !Hide.Special"].setfocus
      if (state.isfocus = 0) then
        foreach pick in hero from Power where "PowerType.AtWill & PowerSrc.Arcane"
          perform eachpick.field[pwDamBonus].modify[+,2,""]
          nexteach
        endif]]></eval>
    </thing>
  <thing id="ftArcanRit" name="Arcane Ritualist" description="{b}Benefit{/b}: Choose a ritual that you know. Change the key skill of that ritual to Arcana and gain a +2 bonus to skill checks made as part of that ritual.{br}Each time you learn a new ritual, you can change the ritual affected by this feat to the newly learned ritual.{br}{b}Special{/b}: You can select this feat more than once. Each time you select this feat, choose a different ritual." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, any arcane class"/>
    <fieldval field="usrCandid1" value="component.Ritual"/>
    <usesource source="Arcane"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    <tag group="Tier" tag="Paragon"/>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[RitualSkl.skArcana]</eval>
    </thing>
  <thing id="ftArcanSpe" name="Arcane Spellfury" description="{b}Benefit{/b}: When you hit any enemy with a sorcerer at-will attack power, you gain a +1 bonus to attack rolls against that enemy until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Sorcerer"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    </thing>
  <thing id="ftArcanTra" name="Arcane Trapsmith" description="{b}Benefit{/b}: You can use your Intelligence modifier instead of your Dexterity modifier when making Thievery checks to disable traps or open locks. You gain a +4 feat bonus to Arcana checks related to traps or hazards." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Artificer, training in Thievery"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsArtific"/>
