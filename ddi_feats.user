    <tag group="ReqClass" tag="clsSorcere"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <tag group="ReqClass" tag="clsWizard"/>
    <eval phase="Setup" priority="1000"><![CDATA[
      #thingdescappend[pInvAveLig, "{b}Addition by Reaper's Touch{/b}: This power gains a range of Melee touch in addition to its normal range. You can use it as a melee basic attack."]
      #thingdescappend[pSorAciOrb, "{b}Addition by Reaper's Touch{/b}: This power gains a range of Melee touch in addition to its normal range. You can use it as a melee basic attack."]
      #thingdescappend[pSorDragon, "{b}Addition by Reaper's Touch{/b}: This power gains a range of Melee touch in addition to its normal range. You can use it as a melee basic attack."]
      #thingdescappend[pWlkEldBla, "{b}Addition by Reaper's Touch{/b}: This power gains a range of Melee touch in addition to its normal range. You can use it as a melee basic attack."]
      #thingdescappend[pWizMagMis, "{b}Addition by Reaper's Touch{/b}: This power gains a range of Melee touch in addition to its normal range. You can use it as a melee basic attack."]]]></eval>
    </thing>
  <thing id="ftReapiBla" name="Reaping Blade" description="{b}Benefit{/b}: If you reduce an enemy to 0 hit points with an attack using a heavy blade, you can shift as a minor action until the end of your current turn." compset="Feat" uniqueness="useronce">
