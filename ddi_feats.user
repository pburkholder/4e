      perform hero.assign[ImplemType.itWand]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftBardiWay" name="Bardic Wayfarer" description="{b}Benefit{/b}: Once per turn when you teleport an ally with a power, you can also teleport 1 square as a free action.{br}In addition, when you use majestic word you can teleport the target 1 square instead of sliding the target." compset="Feat" uniqueness="useronce">
