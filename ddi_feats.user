  <thing id="ftBanesTyr" name="Bane&apos;s Tyrannical Transference" description="{b}Benefit{/b}: You can invoke the power of your deity to use Bane&apos;s tyrannical transference." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship Bane"/>
