    <usesource source="DarkSun"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rMul"/>
    </thing>
  <thing id="ftLegacWar" name="Legacy of War" description="{b}Benefit{/b}: You add your Constitution modifier to the resistance provided by your stone&apos;s endurance racial power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, goliath, stone&apos;s endurance"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rGoliath"/>
    </thing>
  <thing id="ftLegioMoi" name="Legioncaller of Moil" description="{b}Benefit{/b}: Any creature that you summon with a shadow summoning power gains a +1 feat bonus to attack rolls and a +1 feat bonus to all defenses." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsShadow"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftLendHea" name="Lend Health" description="{b}Benefit{/b}: Whenever you use a power that allows an ally to spend a healing surge or regain hit points as if he or she had spent a healing surge, that ally can use your healing surge value to determine the number of hit points regained." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Paladin, lay on hands"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsPaladin"/>
    <prereq message="&apos;Lay on hands&apos; feature required">
      <validate>perform hero.childfound[pPalLayOn].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftLendMig" name="Lend Might" description="{b}Benefit{/b}: When an ally makes an attack granted by one of your warlord powers to attack an enemy adjacent to you, that ally gains a +1 bonus to the attack roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlord"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlord"/>
    </thing>
  <thing id="ftLendStr" name="Lend Strength" description="{b}Benefit{/b}: Whenever you grant an ally a basic attack against an enemy that is adjacent to you, the ally gains a +2 bonus to the attack&apos;s damage roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlord"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlord"/>
    </thing>
  <thing id="ftLethaHam" name="Lethal Hammer Training" description="{b}Benefit{/b}: You can use a warhammer or a throwing hammer with Sneak Attack or any rogue power that normally requires a light blade." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dwarf, rogue"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDwarf"/>
    <tag group="ReqClass" tag="clsRogue"/>
    </thing>
  <thing id="ftLethaHun" name="Lethal Hunter" description="{b}Benefit{/b}: The extra damage dice from your Hunter&apos;s Quarry class feature increase from d6s to d8s." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ranger, Hunter&apos;s Quarry class feature"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    <prereq message="&apos;Hunter&apos;s Quarry&apos; feature required">
      <validate>perform hero.childfound[pRgrHunQua].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftLethaShr" name="Lethal Shroud" description="{b}Benefit{/b}: The damage dealt per shroud when you invoke your shrouds increases from 1d6 to 1d8. This damage increases to 1d8 + 5 at 11th level and 1d8 + 10 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Assassin, assassin&apos;s shroud power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsAssassi"/>
    </thing>
  <thing id="ftLiberSha" name="Liberating Shard Swarm" description="{b}Benefit{/b}: When you use your shard swarm, you can also make a saving throw." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shardmind, shard swarm racial power"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShardmind"/>
    <prereq message="&apos;Shard swarm&apos; feature required">
      <validate>perform hero.childfound[pRacShaSwa].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftLiberTel" name="Liberating Teleport" description="{b}Benefit{/b}: If you teleport on your turn, you gain a +2 feat bonus to saving throws at the end of that turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftLifeon" name="Life on the Edge" description="{b}Benefit{/b}: Whenever you spend an action point while you are bloodied, you gain a +2 bonus to all rolls you make during the granted extra action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shadar-kai"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShadarkai"/>
    </thing>
  <thing id="ftLifeThi" name="Life Thief" description="{b}Benefit{/b}: When you deal necrotic damage with your dark reaping racial power with an attack that also deals Sneak Attack damage, you can reroll each Sneak Attack damage die that shows a result of 1 or 2. Reroll until a given die has a result greater than 2." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Revenant, rogue"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rRevenant"/>
    <tag group="ReqClass" tag="clsRogue"/>
    </thing>
  <thing id="ftLifesHea" name="Lifespirit Healing" description="{b}Benefit{/b}: When you use your second wind, a second ally within 5 squares of you can spend a healing surge and make a saving throw." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, warden, Lifespirit class feature"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsWarden"/>
    <prereq message="&apos;Lifespirit&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWrdLifesp"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftLifesVig" name="Lifespirit Vigor" description="{b}Benefit{/b}: You can grant a saving throw to a second ally within 5 squares of you when you use your second wind." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warden, Lifespirit class feature"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarden"/>
    <prereq message="&apos;Lifespirit&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWrdLifesp"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftLgBlExp" name="Light Blade Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls that you make with a light blade. In addition, you gain a +1 bonus to the damage rolls of weapon attacks that you make with a light blade against a creature granting combat advantage to you. Both of these bonuses increase to +2 at 11th level and +3 at 21st level." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsHeroFa"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgLightBl"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftLightBla" name="Light Blade Mastery" description="{b}Benefit{/b}: When you make a melee weapon attack with a light blade, you can score a critical hit on a natural roll of 19 or 20." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Str 17, Dex 21"/>
    <fieldval field="reqStr" value="17"/>
    <fieldval field="reqDex" value="21"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    </thing>
  <thing id="ftLightPre" name="Light Blade Precision" description="{b}Benefit{/b}: You gain a +2 bonus to damage rolls with light blades against Large or larger targets." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Dex 13, Small or Medium size"/>
    <fieldval field="reqDex" value="13"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="Tier" tag="Paragon"/>
    </thing>
  <thing id="ftLightStr" name="Light Blade Strike" description="{b}Benefit{/b}: When you make a melee basic attack with a light blade against a target&apos;s AC, you can use power strike before making the attack roll. Instead of gaining the power&apos;s normal benefit, you make the attack against the target&apos;s Reflex, rather than AC. In addition, if the attack hits, you gain a +2 bonus to the damage roll against the target. This bonus increases to +4 at 11th level and +6 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Power strike power"/>
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftLightOrd" name="Light of Order" description="{b}Benefit{/b}: As a minor action, you can shed bright light out to 4 squares until you end the light as a free action.{br}In addition, you can swap one 2nd-level or higher utility power for the punitive radiance power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtPuniRad"></bootstrap>
    </thing>
  <thing id="ftLightSte" name="Light Step" description="{b}Benefit{/b}: For the purpose of hourly or daily travel rates, add 1 to your overland speed and the speed of all allies in your traveling group.{br}     Add 5 to the DC required to find or follow your tracks. If traveling with allies, you can share this benefit with up to five other characters.{br}     You gain a +1 feat bonus to Acrobatics checks and Stealth checks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Elf"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rElf"/>
    </thing>
  <thing id="ftLightWit" name="Light Within" description="{b}Benefit{/b}: You gain the Channel Divinity power light within." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship il-Yannah"/>
    <usesource source="EbPG"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtLighWit"></bootstrap>
    </thing>
  <thing id="ftLightWar" name="Light-Footed Warrior" description="{b}Benefit{/b}: When you use a fighter attack power that allows you to move, you ignore difficult terrain during that movement." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Elf, fighter"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rElf"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftLightArc" name="Lightning Arc" description="{b}Benefit{/b}: When you score a critical hit with a power that has the lightning keyword, you can choose to treat the attack as a normal hit instead of a critical hit. If you do, choose another target within 10 squares of the original target that was not already targeted or affected by the power. That target takes damage equal to the damage dealt to the original target." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftLightRef" name="Lightning Reflexes" description="{b}Benefit{/b}: You gain a +2 feat bonus to Reflex. This bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 2
      elseif (#level[] <= 20) then
        bonus = 3
      else
        bonus = 4
        endif
      #traitmodify[defRef,trtFeat,bonus,""]]]>
      <before name="Derived trtFinal"/>
