    <tag group="ReqRace" tag="rWilden"/>
    </thing>
  <thing id="ftSurgiFla" name="Surging Flame" description="{b}Benefit{/b}: When you hit a target that has fire resistance with a fire power, any fire power deals 5 extra fire damage against that target until the end of your next turn." compset="Feat" uniqueness="useronce">
