    <fieldval field="reqCon" value="13"/>
    <fieldval field="reqCha" value="13"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="clsWarlock"/>
    <bootstrap thing="pWlkWarCur"></bootstrap>
    <eval phase="Initialize" priority="1000">~ignore if partial
      perform hero.child[pWlkWarCur].setfocus
      perform focus.delete[PowerUse.AtWill]
      perform focus.assign[PowerUse.Encounter]</eval>
    </thing>
  <thing id="ftStudeMoi" name="Student of Moil" description="{b}Benefit{/b}: You gain a +1 feat bonus to damage rolls when you use a power that has the cold or necrotic keyword.{br}At 11th level, this bonus increases to +2. At 21st level, it increases to +3." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Con 13, Int 13"/>
    <fieldval field="reqCon" value="13"/>
    <fieldval field="reqInt" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftStudeSMg" name="Student of Sword Magic" description="{b}Benefit{/b}: You have a spellbook in which you record your swordmage spells.{br}When you choose this feat, select a second swordmage daily attack power for each level at which you already know a swordmage daily attack power, and add those powers to your spellbook. After an extended rest, you can prepare a number  of daily spells according to what you can cast per day for your level. You can&apos;t prepare the same spell twice.{br}Whenever you learn a new level of swordmage daily attack powers, add two different swordmage daily attack spells of that level to your spellbook.{br}If you retrain a daily swordmage attack power, you replace only one of your two choices for that level. If you replace the power with a power from a different class (through multiclassing) or a swordmage daily attack power of a higher level, you replace both spell choices with the new power or powers." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Swordmage"/>
    <usesource source="FRPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsSwordma"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Setup" priority="2">hero.childfound[clsSwordma].field[clsDailyMl].value = 2
      <before name="Daily powers multiplied"/>
      </eval>
    </thing>
  <thing id="ftStudeAth" name="Student of the Athanaeum" description="{b}Benefit{/b}: Once per day when you use a warlock daily power, you regain the use of that daily power at the end of the encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Int 15, warlock"/>
    <fieldval field="reqInt" value="15"/>
    <usesource source="Dragon"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <tag group="PowerUse" tag="Daily"/>
    </thing>
  <thing id="ftStudeCos" name="Student of the Cosmos" description="{b}Benefit{/b}: You gain an additional benefit based on your current phase in the cosmic cycle.{br}Phase of the Sun: You gain a +2 feat bonus to initiative checks and Heal checks.{br}Phase of the Moon: You gain a +2 feat bonus to Bluff checks and Stealth checks.{br}Phase of the Stars: You gain a +2 feat bonus to Insight checks and Perception checks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Sorcerer, Cosmic Magic class feature"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    <prereq message="&apos;Cosmic Magic&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fSorCosMag"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftStudePla" name="Student of the Plague" description="{b}Benefit{/b}: You gain the ability to focus your spellscar to useful ends. You qualify for the Novice Power, Acolyte Power, and Adept Power feats, treating spellscarred as the class into which you have multiclassed. You also choose one trait from the following list. (Your DM might create other traits to add to this list, or allow you to devise one of your own.){br}* Sight of the Unseen: You have darkvision extending 1 square in all directions that you can turn on and off at will. When your darkvision is on, your eyes are backlit with a dancing blue flame.{br}* Flamespeed: Once per encounter as a minor action, you gain a +1 bonus to speed or a +2 bonus to speed if you charge or run. The bonus lasts until the end of your next turn. Blue flame momentarily licks the ground where your feet touch it.{br}* Larger than Life: Once per day as a free action, your hands flare with a blue glow and you add 1 to the reach of an attack you make." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="You have a spellscar"/>
    <arrayval field="usrArray" index="0" value="Sight of the Unseen"/>
    <arrayval field="usrArray" index="1" value="Flamespeed"/>
    <arrayval field="usrArray" index="2" value="Larger than Life"/>
    <usesource source="FRPG"/>
    <tag group="Multiclass" tag="clsSplscar" name="Spellscarred"/>
    <tag group="Multiclass" tag="clsSplscar"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftStudeSwo" name="Student of the Sword" description="{b}Benefit{/b}: You gain training in one skill from the fighter&apos;s class skill list.{br}Choose either one-handed melee weapons or two-handed melee weapons. Once per encounter as a free action, you can add a +1 bonus to the next attack roll you make with a weapon of that category. Whether the attack hits or misses, you mark the target until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 13"/>
    <fieldval field="reqStr" value="13"/>
    <fieldval field="usrCandid1" value="component.Skill &amp; (thingid.skAthletic | thingid.skEnduranc | thingid.skHeal | thingid.skIntimida | thingid.skStreetwi)"/>
    <tag group="Multiclass" tag="clsFighter"/>
    <tag group="PowerUse" tag="Encounter"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[Helper.TrainedAut]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftSturdBea" name="Sturdy Beast" description="{b}Benefit{/b}: Your beast companion gains one additional healing surge as well as additional hit points equal to 5 + your Wisdom modifier." compset="Feat" uniqueness="useronce">
