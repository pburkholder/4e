  <thing id="ftSunspHer" name="Sunspray Heritage" description="{b}Benefit{/b}: You gain the bathed in the light encounter utility power. Additionally, you gain a +2 feat bonus to Endurance checks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Human"/>
