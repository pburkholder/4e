    <tag group="ReqRace" tag="rElf"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    <prereq message="&apos;Elven accuracy&apos; feature required">
      <validate>perform hero.childfound[pRacElvAcc].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPsionCel" name="Psionic Celerity" description="{b}Benefit{/b}: Whenever you run or charge, you gain a +1 bonus to speed.{br}During a move action or a charge, you can spend 1 power point to gain a +2 bonus to speed until the end of your turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPsionCom" name="Psionic Complement" description="{b}Benefit{/b}: You can swap one of your augmentable at-will attack powers for an augmentable at-will attack power of the same level or lower from the class you multiclassed into. You can use that power once per encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="4th level, any class-specific multiclass feat for a class that has the Psionic Augmentation class feature, Psionic Augmentation class feature"/>
    <usesource source="PHB3"/>
    <tag group="ftDescript" tag="MultiEnc" name="Multiclass Encounter"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="4"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Setup" priority="10000">~decrease our number of class at-will powers by 1
      #resmax[resAtWill] -= 1

      ~increase our number of multiclass at-will powers by 1
      #resmax[resMCAtW] += 1</eval>
    </thing>
  <thing id="ftPsionCon" name="Psionic Conventionalist" description="{b}Benefit{/b}: You can swap one of your augmentable at-will attack powers for an encounter attack power of the same level or lower from the class you multiclassed into. You lose a number of power points based on the level of the at-will attack power: level 1-10, 2 power points; level 11-20, 4 power points; and level 21-30, 6 power points." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="4th level, any class-specific multiclass feat for a class that doesn&apos;t have the Psionic Augmentation class feature, Psionic Augmentation class feature"/>
    <usesource source="PHB3"/>
    <tag group="ftDescript" tag="MultiEnc" name="Multiclass Encounter"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="4"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Setup" priority="10000"><![CDATA[
      ~decrease our number of class at-will powers by 1
      #resmax[resAtWill] -= 1

      ~increase our number of multiclass encounter powers by 1
      #resmax[resMCEnc] += 1

      ~we lose PP too
      var pp as number
      if (#level[] <= 10) then
        pp = 2
      elseif (#level[] <= 20) then
        pp = 4
      else
        pp = 6
        endif
      #trkmax[trkPower] -= pp]]></eval>
    </thing>
  <thing id="ftPsionDab" name="Psionic Dabbler" description="{b}Benefit{/b}: You can swap one of your encounter attack powers for an augmentable at-will attack power of the same level or lower from the class you multiclassed into. You can use that power once per encounter. You gain a number of power points based on the level of the at-will attack power: level 1-10, 2 power points; level 11-20, 4 power points; and level 21-30, 6 power points." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="4th level, any class that doesn&apos;t have the Psionic Augmentation class feature, any class specific multiclass feat for a class that has the Psionic Augmentation class feature"/>
    <usesource source="PHB3"/>
    <tag group="ftDescript" tag="MultiEnc" name="Multiclass Encounter"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="4"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Setup" priority="10000"><![CDATA[
      ~decrease our number of class encounter powers by 1
      #resmax[resEncount] -= 1

      ~increase our number of multiclass at-will powers by 1
      #resmax[resMCAtW] += 1

      ~we gain PP too
      var pp as number
      if (#level[] <= 10) then
        pp = 2
      elseif (#level[] <= 20) then
        pp = 4
      else
        pp = 6
        endif
      #trkmax[trkPower] += pp]]></eval>
    <exprreq message="Requires at least one total class encounter power"><![CDATA[
      #resmax[resEncount] > 0]]></exprreq>
    </thing>
  <thing id="ftPsionDef" name="Psionic Defiling" description="{b}Benefit{/b}: You gain the arcane defiling power and can use it with psionic daily attack powers." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, at least one psionic daily attack power"/>
    <usesource source="DarkSun"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPsionDis" name="Psionic Discorporation" description="{b}Benefit{/b}: While you are at 0 hit points or fewer, you are insubstantial. When you regain hit points so that you are no longer dying, you gain a +2 bonus to attack rolls for psionic powers until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Elan Heritage"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="ElanBloodl" name="Elan Bloodline"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <exprreq message="Feat &apos;Elan Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftElanHer] <> 0]]></exprreq>
    </thing>
  <thing id="ftPsionFer" name="Psionic Ferocity" description="{b}Benefit{/b}: If you have at least 1 power point when you would make a melee basic attack using your Ferocity racial trait, you can expend that power point to deal 1[W] extra damage if it is a weapon attack or 1d8 extra damage if it isn&apos;t." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Minotaur, Ferocity racial trait, any psionic class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rMinotaur"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    </thing>
  <thing id="ftPsionFor" name="Psionic Fortune" description="{b}Benefit{/b}: While you have at least 1 power point, you gain a +1 feat bonus to saving throws.{br}When you make a saving throw, you can spend 1 power point to increase the feat bonus to +3 until the end of your turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPsionFur" name="Psionic Fury" description="{b}Benefit{/b}: Your furious assault power gains the augmentable keyword. When you would use furious assault, you can spend 1 power point to deal damage to each enemy adjacent to you equal to the higher of your Strength or Dexterity modifier. This damage has the same damage type as the attack that triggered this power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Half-orc, furious assault racial power, any psionic class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rHalfOrc"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    <prereq message="&apos;Furious assault&apos; feature required">
      <validate>perform hero.childfound[pRacFurAss].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPsionIni" name="Psionic Initiative" description="{b}Benefit{/b}: You gain a +3 feat bonus to initiative checks.{br}When you roll initiative, you can spend 1 power point to increase the bonus to +6." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[trInit,trtFeat,3,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPsionRef" name="Psionic Reflexes" description="{b}Benefit{/b}: You gain a +1 bonus to opportunity attack rolls.{br}When you make an opportunity attack, you can spend 1 power point to gain a +2 power bonus to the attack roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPsionRej" name="Psionic Rejuvenation" description="{b}Benefit{/b}: When you use your second wind, you also regain 1 power point." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, shardmind, Psionic Augmentation class feature"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rShardmind"/>
    </thing>
  <thing id="ftPsionRus" name="Psionic Rush" description="{b}Benefit{/b}: When you spend an action point to take an extra action, you can reduce the cost to augment any augmentable power you use during that action by 1 power point." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPsionSco" name="Psionic Scorn" description="{b}Benefit{/b}: When a nonminion enemy cursed by you drops to 0 hit points, you can regain 1 power point instead of regaining your fell might." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, sorcerer-king pact, 1 or more power points"/>
    <usesource source="DarkSun"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <prereq message="&apos;Sorcerer-king pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkSorPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPsionSki" name="Psionic Skill" description="{b}Benefit{/b}: Choose any skill. You gain a +2 feat bonus to checks with that skill.{br}When you make a check with that skill, you can spend 1 power point to gain a +3 power bonus to the skill check.{br}{b}Special{/b}: You can take this feat more than once. Each time you take this feat, choose a different skill." compset="Feat">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <fieldval field="usrCandid1" value="component.Skill"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Augment"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform field[usrChosen1].chosen.field[trtFeat].modify[+,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPsionSta" name="Psionic Staff Focus" description="{b}Benefit{/b}: When you use a ranged psion attack power using a staff against a target adjacent to you, the attack does not provoke opportunity attacks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, psion"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPsionTou" name="Psionic Toughness" description="{b}Benefit{/b}: When you take this feat, you gain additional hit points. You gain 3 additional hit points at each tier of play (at 1st, 11th, and 21st level).{br}In addition, when you use your second wind, you can spend 1 power point to regain 5 additional hit points. The additional hit points regained increase to 10 at 11th level and 15 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (hero.tagvalue[Level.?] <= 10) then
        bonus = 3
      elseif (hero.tagvalue[Level.?] <= 20) then
        bonus = 6
      else
        bonus = 9
        endif
      #traitmodify[trHealth,trtBonus,bonus,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPsionTri" name="Psionic Trick" description="{b}Benefit{/b}: You do not provoke opportunity attacks from enemies granting combat advantage to you." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Changeling, changeling trick racial power, any psionic class"/>
    <fieldval field="reqCha" value="0"/>
    <usesource source="Dragon"/>
    <tag group="ReqRace" tag="rChangelin"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    </thing>
  <thing id="ftPsionVam" name="Psionic Vampire" description="{b}Benefit{/b}: Once per encounter, when your augmented psionic attack power or psionic encounter attack power hits at least one enemy, you gain a healing surge.{br}Once per encounter, as a minor action, you can lose a healing surge to gain 2 power points that expire at the end of your turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Vampire, Psionic Augmentation class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="PowerUse" tag="Encounter"/>
    </thing>
  <thing id="ftPsychBas" name="Psychic Bastion" description="{b}Benefit{/b}: Whenever you score a critical hit with a psionic psychic power, you gain a +4 bonus to all defenses until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, psion"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPsychCor" name="Psychic Corruption of Malbolge" description="{b}Benefit{/b}: After you hit an enemy with an augmented psionic power, that enemy gains vulnerability to the fire and the psychic damage of your powers until the end of your next turn. The vulnerability equals 2 and increases to 4 at 11th level and 6 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling, any psionic class"/>
    <usesource source="PHRcTief"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    </thing>
  <thing id="ftPsychFea" name="Psychic Feast" description="{b}Benefit{/b}: While you benefit from longtooth shifting and when you hit with an attack on your turn, your regeneration increases by 2 until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Longtooth shifter, longtooth shifting racial power, any psionic class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShifteLon"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    <prereq message="&apos;Longtooth shifting&apos; feature required">
      <validate>perform hero.childfound[pRacLonShi].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPsychFoc" name="Psychic Focus" description="{b}Benefit{/b}: You gain a +2 feat bonus to damage rolls with psychic powers. The bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shardmind"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShardmind"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 2
      elseif (#level[] <= 20) then
        bonus = 3
      else
        bonus = 4
        endif
      foreach pick in hero from Power where "DamageType.Psychic"
        perform eachpick.field[pwDamFeat].modify[+,bonus,""]
        nexteach]]></eval>
    </thing>
  <thing id="ftPsychLoc" name="Psychic Lock" description="{b}Benefit{/b}: Any target you hit with a power that has the psychic keyword takes a -2 penalty to its next attack roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPsychRet" name="Psychic Retaliation" description="{b}Benefit{/b}: When you use your bastion of mental clarity racial power, you can make the triggering enemy your new oath of enmity target as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Kalashtar, avenger, oath of enmity power"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rKalashtar"/>
    <tag group="ReqClass" tag="clsAvenger"/>
    </thing>
  <thing id="ftPsychWai" name="Psychic Wail" description="{b}Benefit{/b}: When you reduce an enemy to 0 hit points with a psychic attack, one enemy you can see within 5 squares of the first enemy grants combat advantage to you until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, any psionic class"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqPwrSrc" tag="Psionic"/>
    </thing>
  <thing id="ftPulseLif" name="Pulse of Life" description="{b}Benefit{/b}: You gain the Channel Divinity power pulse of life." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Channel Divinity class feature, must worship a deity of the life domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtPulsLif"></bootstrap>
    </thing>
  <thing id="ftPulseLig" name="Pulse of Lightning" description="{b}Benefit{/b}: When you use a wilden racial power, until the start of your next turn, the first enemy that targets you with a close or area attack takes lightning damage equal to your Wisdom modifier. The lightning damage equals 3 + your Wisdom modifier at 11th level, and it equals 6 + your Wisdom modifier at 21st level.{br}If you are in the aspect of the elements, one other enemy you can see also takes lightning damage equal to half that taken by the first enemy." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wilden, Nature&apos;s Aspect racial trait"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rWilden"/>
    </thing>
  <thing id="ftPunisAxe" name="Punishing Axe" description="{b}Benefit{/b}: Whenever you score a critical hit using an axe, you treat rolls of 1 or 2 on the critical hit damage dice and the extra dice provided by the high crit property as rolls of 3." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, any martial class"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    </thing>
  <thing id="ftPunisRad" name="Punishing Radiance" description="{b}Benefit{/b}: Whenever you score a critical hit with a divine attack power that has the radiant keyword, the target and each enemy within 5 squares of it gain vulnerable 10 radiant (or its existing vulnerability to radiant damage increases by 10) until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, any divine class"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    </thing>
  <thing id="ftPunisReb" name="Punishing Rebuke" description="{b}Benefit{/b}: Your spirits&apos; rebuke deals 1[W] extra damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, seeker, spirits&apos; rebuke power"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsSeeker"/>
    </thing>
  <thing id="ftPunisSpi" name="Punishing Spike" description="{b}Benefit{/b}: When you damage a target with your mind spike, that target takes a -2 penalty to the next saving throw it makes before the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Battlemind, mind spike power"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBattlem"/>
    </thing>
  <thing id="ftPupilAll" name="Pupil of the All-Father" description="{b}Benefit{/b}: You have mastered the Enchant Magic Item and Make Whole rituals, and can perform them without a ritual book.{br}When using Enchant Magic Item to enchant weapons or armor, you perform the ritual as if you were four levels higher.{br}Once per day, you can perform the Make Whole ritual without expending components, as long as the component cost would have been 100 gp or fewer." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class, must worship Moradin"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <tag group="ReqDeity" tag="dtyMoradin"/>
    <bootstrap thing="rtEnchaMag"></bootstrap>
    <bootstrap thing="rtMakeWho"></bootstrap>
    </thing>
  <thing id="ftPuppetee" name="Puppeteer" description="{b}Benefit{/b}: Whenever an enemy fails a saving throw against an effect caused by one of your psion attack powers, you can slide that enemy 1 square as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, psion"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPureMin" name="Pure Mind" description="{b}Benefit{/b}: You gain resist 10 psychic and a +2 feat bonus to Will." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[rsPsychic,rsTotal,10,""]
      #traitmodify[defWill,trtFeat,2,""]]]></eval>
    </thing>
  <thing id="ftPurgiSpi" name="Purging Spirit" description="{b}Benefit{/b}: When you use healing spirit, each ally adjacent to your spirit companion can make a saving throw." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, shaman, healing spirit power"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsShaman"/>
    </thing>
  <thing id="ftPurifLig" name="Purifying Light" description="{b}Benefit{/b}: Your radiant prayers gain a +1 bonus to attack rolls when used against undead targets. At 11th level, this bonus increases to +2. At 21st level, this bonus increases to +3." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Cha 15, any divine class"/>
    <fieldval field="reqCha" value="15"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    </thing>
  <thing id="ftPursuSte" name="Pursuing Step" description="{b}Benefit{/b}: When you use blurred step, if you end the shift adjacent to the triggering enemy, that enemy grants combat advantage to you until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Battlemind, blurred step power"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBattlem"/>
    </thing>
  <thing id="ftQuickDea" name="Quick and Dead" description="{b}Benefit{/b}: When you use dark reaping, increase the distance you can teleport with shadow step by 1 square until the end of the encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Revenant, dark reaping, assassin, shadow step power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rRevenant"/>
    <tag group="ReqClass" tag="clsAssassi"/>
    </thing>
  <thing id="ftQuickBea" name="Quick Beast Command" description="{b}Benefit{/b}: On your turn, you can use a minor action to command your beast companion to make a melee basic attack." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, Wis 17, ranger, Beast Mastery class feature"/>
    <fieldval field="reqWis" value="17"/>
    <usesource source="Martial"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    <prereq message="&apos;Beast Mastery&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fRgrBeaMas"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftQuickDef" name="Quick Defense" description="{b}Benefit{/b}: When you use your quick formation, any character who shifts as a result of the power gains a +2 bonus to all defenses until the end of your next turn." compset="Feat" uniqueness="useronce">
