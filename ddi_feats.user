    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsCleric"/>
    </thing>
  <thing id="ftBattlIns" name="Battle Instructor" description="{b}Benefit{/b}: You gain the warlord&apos;s Combat Leader class feature." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any multiclass warlord feat, paragon multiclassing as a warlord"/>
