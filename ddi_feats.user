      <before name="Armor type proficiencies"/>
      </eval>
    </thing>
  <thing id="ftBattlHar" name="Battle Hardened" description="{b}Benefit{/b}: You gain a +5 feat bonus to saving throws against fear effects. In addition, you gain a +2 feat bonus to initiative checks." compset="Feat" uniqueness="useronce">
