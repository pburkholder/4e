      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgBow"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
