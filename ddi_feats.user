  <thing id="ftEmpowRea" name="Empowered Reaping" description="{b}Benefit{/b}: You gain a +2 feat bonus to the damage dealt by your dark reaping. This bonus increases to +4 at 11th level and +6 at 21st level." compset="Feat" uniqueness="useronce">
