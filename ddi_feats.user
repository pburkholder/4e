    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtFuriDem"></bootstrap>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~+2 intimidate
      #traitmodify[skIntimida,trtFeat,2,""]]]></eval>
    </thing>
  <thing id="ftFurioDev" name="Furious Devotion" description="{b}Benefit{/b}: When you use a Channel Divinity power, you gain a +2 bonus to damage rolls with divine attack powers until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Half-orc, any divine class, Channel Divinity class feature"/>
