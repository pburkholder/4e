  <thing id="ftPowerUnd" name="Power of Undeath" description="{b}Benefit{/b}: You gain a +2 feat bonus to Religion checks.{br}When you use a power associated with this feat, the power&apos;s damage changes to necrotic (the power gains the necrotic keyword and loses the keywords of its former damage types). You deal 2 extra damage. The extra damage increases to 3 at 11th level and 4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class, must worship a deity of the undeath domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Domain"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skReligion,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPowerVen" name="Power of Vengeance" description="{b}Benefit{/b}: You gain a +2 feat bonus to Intimidate checks.{br}When you use a power associated with this feat and hit an enemy with it, that enemy takes 2 damage the first time it attacks you or an ally of yours before the start of your next turn. The damage increases to 3 at 11th level and 4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class, must worship a deity of the vengeance domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Domain"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skIntimida,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPowerWar" name="Power of War" description="{b}Benefit{/b}: You gain a +2 feat bonus to History checks.{br}When you use a power associated with this feat against an unbloodied target, you gain a +1 bonus to the attack roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class, must worship a deity of the war domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Domain"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skHistory,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPowerWin" name="Power of Winter" description="{b}Benefit{/b}: You gain a +2 feat bonus to Endurance checks.{br}When you use a power associated with this feat, the power&apos;s damage changes to cold (the power gains the cold keyword and loses the keywords of its former damage types). You also gain a +2 bonus to the damage roll. The bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any divine class, must worship a deity of the winter domain"/>
    <usesource source="Divine"/>
    <tag group="ftDescript" tag="Domain"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skEnduranc,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPowerSpe" name="Power Strike Specialization" description="{b}Benefit{/b}: Choose one of the 7th level Weapon Specialization options of the knight or the slayer, such as Bladed Step or Sweeping Sword. You gain that class feature." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="7th level, fighter (weaponmaster), power strike power"/>
    <usesource source="ClassComp"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="7"/>
    </thing>
  <thing id="ftPowerSur" name="Power Surge" description="{b}Benefit{/b}: You can expend the use of an unused daily psion attack power to regain 2 power points. If the daily power is 15th level or higher, you instead regain 4 power points. If the daily power is 25th level or higher, you instead regain 6 power points." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, psion"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPowerThr" name="Power Throw" description="{b}Benefit{/b}: When you make a ranged attack with a heavy thrown weapon, you can take a -2 penalty to the attack roll. If the attack hits, you gain a +2 bonus to the damage roll. The bonus increases to +4 at 11th level and +6 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 15"/>
    <fieldval field="reqStr" value="15"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPowerBre" name="Powerful Breath" description="{b}Benefit{/b}: Choose an ability score. Use this ability score for attack and damage rolls for your dragon breath from now on." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Dragonborn, dragon breath racial power"/>
    <fieldval field="usrCandid1" value="component.Attribute"/>
    <usesource source="PHRcDragon"/>
    <tag group="ReqRace" tag="rDragonbor"/>
    <tag group="Hide" tag="Special"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.setfocus
      doneif (state.isfocus = 0)
      perform focus.pulltags[Attack.?]
      perform focus.pulltags[DamageAttr.?]
      perform hero.childfound[pRacDraBre].setfocus
      doneif (state.isfocus = 0)
      perform focus.delete[Attack.?]
      perform focus.delete[DamageAttr.?]
      perform focus.pushtags[Attack.?]
      perform focus.pushtags[DamageAttr.?]
      <before name="Calc sklTrained"/>
      </eval>
    <prereq message="&apos;Dragon breath&apos; feature required">
      <validate>perform hero.childfound[pRacDraBre].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPwrflChr" name="Powerful Charge" description="{b}Benefit{/b}: When you charge, you gain a +2 bonus to damage and a +2 bonus to bull rush attempts." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 13"/>
    <fieldval field="reqStr" value="13"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftPowerLur" name="Powerful Lure" description="{b}Benefit{/b}: When you use lure of enchantment, you can slide the target up to 6 squares, instead of the normal distance, and you can slide it into hindering terrain." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Level 11, satyr, lure of enchantment racial power"/>
    <usesource source="HeroFeywil"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rSatyr"/>
    <prereq message="&apos;Lure of enchantment&apos; feature required">
      <validate>perform hero.childfound[pRacLurEnc].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPwrflStr" name="Powerful Strike" description="{b}Benefit{/b}: Choose one of your fighter encounter attack powers that has a level. You lose that power and gain the power strike power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any fighter encounter attack power that has a level"/>
    <fieldval field="usrCandid1" value="component.Power &amp; PowerClass.clsFighter &amp; PowerType.Encounter &amp; ReqLevel.?"/>
    <usesource source="ClassComp"/>
    <tag group="Tier" tag="Heroic"/>
    <bootstrap thing="pFtrPowStr">
      <autotag group="User" tag="ClassOK"/>
      </bootstrap>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[Helper.Disable]
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftPractKil" name="Practiced Killer" description="{b}Benefit{/b}: You gain training in one skill from the executioner assassin&apos;s class skills list. You also gain proficiency with ki focuses.{br}Once per encounter, you can deal 1d8 extra damage with a weapon attack using a one-handed weapon, a garrote, a blowgun, or a shortbow. The extra damage increases to 2d8 at 11th level and 3d8 at 21st level." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="clsAssassi"/>
    </thing>
  <thing id="ftPractPro" name="Practiced Prodigy" description="{b}Benefit{/b}: You acquire two martial practices of your level or lower. Increase the number of practices acquired to three at 5th level, four at 11th level, five at 15th level, six at 21st level, and seven at 25th level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Practiced Study feat"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~bonus practices
      var bonus as number
      if (#level[] < 5) then
        bonus = 2
      elseif (#level[] < 11) then
        bonus = 3
      elseif (#level[] < 15) then
        bonus = 4
      elseif (#level[] < 21) then
        bonus = 5
      elseif (#level[] < 25) then
        bonus = 6
      else
        bonus = 7
        endif
      hero.child[resRituals].field[resMax].value += bonus]]></eval>
    <exprreq message="Feat &apos;Practiced Study&apos; required"><![CDATA[hero.tagis[Feat.ftPractStu] <> 0]]></exprreq>
    </thing>
  <thing id="ftPractRel" name="Practiced Reliability" description="{b}Benefit{/b}: Choose a fighter encounter attack power that you know and that has a single target and a single attack roll. That power gains the reliable keyword.{br}{b}Special{/b}: Each time you gain a level, you can choose to remove this feat&apos;s benefit from the power and assign it to a different power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, fighter"/>
    <fieldval field="usrCandid1" value="component.Power &amp; PowerClass.clsFighter &amp; PowerType.Encounter"/>
    <usesource source="Martial"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsFighter"/>
    <tag group="Tier" tag="Epic"/>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[EffectType.Reliable]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftPractStu" name="Practiced Study" description="{b}Benefit{/b}: You can master and perform martial practices. You learn one martial practice of your level or lower." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~we have to "fake" being a ritual caster to get access to any ritual
      ~stuff, even though this gives us too much
      perform hero.assign[Feat.ftRituaCas]

      ~we get one bonus martial practice
      hero.child[resRituals].field[resMax].value += 1]]></eval>
    </thing>
  <thing id="ftPreciFan" name="Precise Fangs" description="{b}Benefit{/b}: When you use your spirit&apos;s fangs power, you gain a +2 bonus to the attack roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shaman, Stalker Spirit class feature"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsShaman"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform hero.child[pShmSpiFan].field[pwAtkMod].modify[+,2,""]]]>
      <before name="Power attack final"/>
      </eval>
    <prereq message="&apos;Stalker Spirit&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fShmStaSpi"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPreciHun" name="Precise Hunter" description="{b}Benefit{/b}: When you score a critical hit against the target of your Hunter&apos;s Quarry with a ranged attack, your allies gain a +1 bonus to attack rolls against that target until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Wis 15, ranger, Hunter&apos;s Quarry class feature"/>
    <fieldval field="reqWis" value="15"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    <prereq message="&apos;Hunter&apos;s Quarry&apos; feature required">
      <validate>perform hero.childfound[pRgrHunQua].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPreciMin" name="Precise Mind" description="{b}Benefit{/b}: Whenever you hit with a psionic attack power that is augmented with 2 or more power points, you gain a +1 bonus to attack rolls with psionic at-will attack powers that are unaugmented until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Psion"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPreciAmb" name="Precision Ambush Style" description="{b}Benefit{/b}: You gain a benefit with any of the following powers you possess.{br}Disheartening Strike (rogue): If you hit with this exploit during a surprise round, the target grants combat advantage to you until the end of your next turn.{br}Footwork Lure (fighter): While you charge during the surprise round, you can use this exploit in place of a melee basic attack.{br}Opening Shove (warlord): While you charge during a surprise round, you can use this exploit in place of a melee basic attack.{br}Twin Strike (ranger): If you hit with this exploit during a surprise round, you deal extra damage equal to your Strength modifier if you are making melee attacks, or your Dexterity modifier if you are making ranged attacks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    <eval phase="Setup" priority="1000"><![CDATA[
      #thingdescappend[pRogDisStr, "{b}Addition by Precision Ambush Style{/b}: If you hit with this exploit during a surprise round, the target grants combat advantage to you until the end of your next turn."]
      #thingdescappend[pFtrFooLur, "{b}Addition by Precision Ambush Style{/b}: While you charge during the surprise round, you can use this exploit in place of a melee basic attack."]
      #thingdescappend[pWrlOpeSho, "{b}Addition by Precision Ambush Style{/b}: While you charge during a surprise round, you can use this exploit in place of a melee basic attack."]
      #thingdescappend[pRgrTwiStr, "{b}Addition by Precision Ambush Style{/b}: If you hit with this exploit during a surprise round, you deal extra damage equal to your Strength modifier if you are making melee attacks, or your Dexterity modifier if you are making ranged attacks."]]]></eval>
    </thing>
  <thing id="ftPreciThr" name="Precision Throw" description="{b}Benefit{/b}: Whenever you make a ranged basic attack using a thrown weapon with which you have proficiency, you can use Dexterity instead of Strength for the attack roll and the damage roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    </thing>
  <thing id="ftPredaBur" name="Predator&apos;s Burst" description="{b}Benefit{/b}: When you charge while you are in beast form, you gain a +2 bonus to speed and a +1 bonus to the damage rolls of the charge attack." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Druid, Primal Predator class feature, wild shape power"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsDruid"/>
    <prereq message="&apos;Primal Predator&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fDrdPriPre"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPredaAct" name="Predatory Action" description="{b}Benefit{/b}: If you spend an action point to take an extra action and have already dealt Hunter&apos;s Quarry damage during this round, you can deal the extra damage a second time during this turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ranger, Hunter&apos;s Quarry class feature"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    <prereq message="&apos;Hunter&apos;s Quarry&apos; feature required">
      <validate>perform hero.childfound[pRgrHunQua].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftPredaMag" name="Predatory Magic" description="{b}Benefit{/b}: When you use a close arcane attack power while you are under the effect of your razorclaw shifting racial power, you can shift 1 square as a free action before or after using the power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Razorclaw shifter, any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rShifteRaz"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftPredaSpi" name="Predatory Spirit" description="{b}Benefit{/b}: When you use longtooth shifting or razorclaw shifting, once before the end of your next turn you can reroll one attack roll with a primal attack power that misses and take either result." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Shifter, longtooth shifting or razorclaw shifting racial power, any primal class"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqPwrSrc" tag="Primal"/>
    </thing>
  <thing id="ftPrediDef" name="Predictive Defense" description="{b}Benefit{/b}: Whenever you hit an enemy with a psionic at-will attack power that is unaugmented, you gain a +2 bonus to all defenses against melee attacks until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, psion"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsPsion"/>
    </thing>
  <thing id="ftPrepaAny" name="Prepared for Anything" description="{b}Benefit{/b}: Any ally who benefits from your Combat Leader class feature gains a +2 bonus to AC until the end of his or her first turn in an encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlord, Combat Leader class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlord"/>
    <prereq message="&apos;Combat Leader&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWrlComLea"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPrescAid" name="Prescient Aid" description="{b}Benefit{/b}: When an ally within 10 squares of you misses with an attack roll, you can use your Virtue of Prescience as an immediate interrupt. That ally gains a power bonus to the attack roll equal to your Wisdom modifier." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, bard, Virtue of Prescience class feature"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsBard"/>
    <prereq message="&apos;Virtue of Prescience&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fBrdVirPre"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPrescDod" name="Prescient Dodge" description="{b}Benefit{/b}: During each encounter, you gain a +2 bonus to all defenses until the start of your first turn.{br}Whenever you hit an enemy with an unaugmented attack power, you can spend 1 power point to gain a +2 power bonus to all defenses until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Psionic Augmentation class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="Augment"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPrescFor" name="Prescient Fortification" description="{b}Benefit{/b}: When you use your Virtue of Prescience to grant a power bonus to an ally, increase that bonus by 2." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Bard, Virtue of Prescience class feature"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBard"/>
    <prereq message="&apos;Virtue of Prescience&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fBrdVirPre"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPrescRes" name="Prescient Resurgence" description="{b}Benefit{/b}: When you spend an action point to gain an extra action, you also regain the use of your Virtue of Prescience." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, bard, Virtue of Prescience class feature"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsBard"/>
    <prereq message="&apos;Virtue of Prescience&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fBrdVirPre"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPrescRet" name="Prescient Retaliation" description="{b}Benefit{/b}: You can use mind spike as an immediate interrupt instead of an immediate reaction." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, battlemind, mind spike power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsBattlem"/>
    </thing>
  <thing id="ftPreseCal" name="Preserver&apos;s Call" description="{b}Benefit{/b}: When you use a divine daily attack power on your turn, you can pull each ally within 5 squares of you 1 square instead of using the normal benefit of your Covenant Manifestation." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Kalashtar, invoker, Covenant of Preservation class feature"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rKalashtar"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    <prereq message="&apos;Covenant of Preservation&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fInvCovPre"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftPreseCom" name="Preserver&apos;s Command" description="{b}Benefit{/b}: When you use preserver&apos;s rebuke, you can forgo the bonus to your next attack roll to instead allow the ally hit by the triggering enemy to make a basic attack." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, invoker, preserver&apos;s rebuke power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    </thing>
  <thing id="ftPreseFur" name="Preserver&apos;s Fury" description="{b}Benefit{/b}: When you use preserver&apos;s rebuke and your next attack roll against the triggering enemy hits, that enemy gains vulnerability to all damage equal to your Intelligence modifier until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, preserver&apos;s rebuke power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    </thing>
  <thing id="ftPreseSum" name="Preserver&apos;s Summons" description="{b}Benefit{/b}: When you use preserver&apos;s rebuke, you can slide the triggering ally 1 square and, if it is marked, you can end that condition." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Invoker, preserver&apos;s rebuke power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    </thing>
  <thing id="ftPreseVen" name="Preserver&apos;s Vengeance" description="{b}Benefit{/b}: You gain a +2 bonus to damage rolls with divine attack powers while a bloodied ally is within 5 squares of you." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, invoker, Covenant of Preservation class feature"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    <prereq message="&apos;Covenant of Preservation&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fInvCovPre"
call ReqFeature]]></validate>
      </prereq>
