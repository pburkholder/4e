    </thing>
  <thing id="ftSlaveto" name="Slave to None" description="{b}Benefit{/b}: You gain a +2 feat bonus to saving throws against effects that dominate, immobilize, or slow." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Escaped slave"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftSlayeShi" name="Slayer&apos;s Shift" description="{b}Benefit{/b}: When you bloody a creature or reduce a creature to 0 hit points, you can shift 1 square as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="21st level, barbarian"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Epic"/>
    <tag group="ReqLevel" tag="21"/>
    <tag group="ReqClass" tag="clsBarbari"/>
    </thing>
  <thing id="ftSlayiAct" name="Slaying Action" description="{b}Benefit{/b}: If you spend an action point to take an extra action and have already dealt Sneak Attack damage during this round, you can deal the extra damage a second time during this turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Rogue, Sneak Attack class feature"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRogue"/>
    <prereq message="&apos;Sneak Attack&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fRogSneAtt"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftSlingExp" name="Sling Expertise" description="{b}Benefit{/b}: You gain a +1 feat bonus to weapon attack rolls that you make with a sling. This bonus increases to +2 at 11th level and +3 at 21st level.{br}In addition, when you make a ranged or an area attack with a sling, you don&apos;t provoke opportunity attacks for doing so." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsHeroFa"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 1
      elseif (#level[] <= 20) then
        bonus = 2
      else
        bonus = 3
        endif
      foreach pick in hero from WeaponBase where "WepGroup.wgSling"
        perform eachpick.field[wpAtkFeat].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
  <thing id="ftSlithFor" name="Slithering Form" description="{b}Benefit{/b}: You do not provoke opportunity attacks when crawling while in beast form.{br}You gain a +2 feat bonus to Acrobatics checks." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Druid, wild shape power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsDruid"/>
    </thing>
  <thing id="ftSlowFal" name="Slow Fall" description="{b}Benefit{/b}: When you fall, you reduce the falling damage you take by your Acrobatics check result instead of one-half your check result." compset="Feat" uniqueness="useronce">
