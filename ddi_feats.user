    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[Helper.TrainedAut]

      perform hero.assign[ImplemType.itWand]
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftBardiKno" name="Bardic Knowledge" description="{b}Benefit{/b}: You gain a +2 feat bonus to Arcana, Dungeoneering, History, Nature, Religion, and Streetwise checks." compset="Feat" uniqueness="useronce">
