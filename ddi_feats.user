    <exprreq message="Feat &apos;Garrote Training&apos; required"><![CDATA[hero.tagis[Feat.ftGarroTra] <> 0]]></exprreq>
    </thing>
  <thing id="ftGarroNov" name="Garrote Novice" description="{b}Benefit{/b}: You can swap one 3rd-level or higher encounter attack power you know for the choking grab attack power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="4th level, Garrote Training"/>
