    <tag group="Tier" tag="Heroic"/>
    <tag group="Multiclass" tag="clsRogue"/>
    </thing>
  <thing id="ftSlyHun" name="Sly Hunter" description="{b}Benefit{/b}: You gain a +3 bonus to damage rolls with bow attacks against any target that has no creature within 3 squares of it." compset="Feat" uniqueness="useronce">
