    <tag group="Tier" tag="Epic"/>
    </thing>
  <thing id="ftQuickFon" name="Quickening Font" description="{b}Benefit{/b}: When you save against an effect at the start of your turn, your speed increases by 1 until the end of your turn." compset="Feat" uniqueness="useronce">
