    <tag group="ReqClass" tag="clsInvoker"/>
    <tag group="ReqDeity" tag="dtyAvandra"/>
    </thing>
  <thing id="ftLuckySho" name="Lucky Shot" description="{b}Benefit{/b}: When you use a daily sorcerer attack power or a daily sorcerer paragon path attack power, for one attack roll choose even numbers or odd numbers. If the result of the d20 roll matches the type of number selected, you gain a feat bonus to the damage roll against the target equal to your Charisma modifier for this attack." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Sorcerer, Wild Magic class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    <prereq message="&apos;Wild Magic&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fSorWilMag"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftLuckySki" name="Lucky Skirmisher" description="{b}Benefit{/b}: When you run, enemies making opportunity attacks against you during that movement must roll twice and take the lower result." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Halfling ranger or halfling rogue"/>
    <usesource source="Martial"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftLuckySta" name="Lucky Start" description="{b}Benefit{/b}: If your initiative is the highest of any combatant&apos;s at the start of an encounter, when you make your first attack roll of the encounter you can roll twice and use either result." compset="Feat" uniqueness="useronce">
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftLureIro" name="Lure of Iron" description="{b}Benefit{/b}: You can slide the target of your mind spike 1 square." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Battlemind, mind spike power"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBattlem"/>
    </thing>
  <thing id="ftLyricRej" name="Lyric of Rejuvenation" description="{b}Benefit{/b}: Each ally affected by your Song of Rest regains 2 additional hit points with each healing surge he or she spends at the end of the rest." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, bard, Song of Rest class feature"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsBard"/>
    <prereq message="&apos;Song of Rest&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fBrdSonRes"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMaceStr" name="Mace Strike" description="{b}Benefit{/b}: When you make a melee basic attack with a mace against a target&apos;s AC, you can use power strike before making the attack roll. Instead of gaining the power&apos;s normal benefit, you make the attack against the target&apos;s Fortitude, rather than AC. In addition, if the attack hits, you gain a +2 bonus to the damage roll against the target. This bonus increases to +4 at 11th level and +6 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Power strike power"/>
    <usesource source="MordensEmp"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftMagicAge" name="Magic of the Ages" description="{b}Benefit{/b}: When you use your memory of a thousand lifetimes racial power to add to the attack roll of an arcane power, you gain a +1 bonus to the triggering roll." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Deva, any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDeva"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftMagicMis" name="Magic of the Mists" description="{b}Benefit{/b}: When you use your fade away racial power, making an attack with an arcane power does not end the effect." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Gnome, any arcane class"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGnome"/>
    <tag group="ReqPwrSrc" tag="Arcane"/>
    </thing>
  <thing id="ftMagmaEru" name="Magma Eruptions" description="{b}Benefit{/b}: Any creature damaged by the aura created by your flowing magma power is also weakened until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Genasi, magmasoul elemental manifestation"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGenasi"/>
    </thing>
  <thing id="ftMajesFer" name="Majestic Fervor" description="{b}Benefit{/b}: If the target of your majestic word is not bloodied, it also gains a +1 bonus to attack rolls until the end of its next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Bard, trained in Religion"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBard"/>
    <tag group="ReqSkill" tag="skReligion"/>
    </thing>
  <thing id="ftMajesPre" name="Majestic Presence" description="{b}Benefit{/b}: Allies adjacent to you gain resist 3 necrotic and resist 3 radiant. The resistance increases to 5 at 11th level and 10 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Deva, any divine class"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDeva"/>
    <tag group="ReqPwrSrc" tag="Divine"/>
    </thing>
  <thing id="ftMajesRes" name="Majestic Rescue" description="{b}Benefit{/b}: When you use majestic word, you can grant the target a saving throw in addition to the other effects of the power." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, bard, majestic word power"/>
    <usesource source="Arcane"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsBard"/>
    </thing>
  <thing id="ftMaledEye" name="Maledictor&apos;s Eye" description="{b}Benefit{/b}: When you hit a target with evil eye of the Vistani, you can slide the target 1 square. Until the end of your next turn, whenever the target misses with an attack roll, you can slide the target 1 square as a free action." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Invoker, Covenant of Malediction class feature, Vistani Heritage"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="VistaniBlo" name="Vistani Bloodline"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsInvoker"/>
    <exprreq message="Feat &apos;Vistani Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftVistaHer] <> 0]]></exprreq>
    <prereq message="&apos;Covenant of Malediction&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fInvCovMal"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMammoThe" name="Mammon&apos;s Theft of Health" description="{b}Benefit{/b}: When you use your second wind, you can choose for an ally within 5 squares of you to lose a healing surge. If you do, you regain extra hit points equal to your healing surge value." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, tiefling"/>
    <usesource source="PHRcTief"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rTiefling"/>
    </thing>
  <thing id="ftManeuAtt" name="Maneuvering Attack" description="{b}Benefit{/b}: When you pull, push, or slide a target with an attack using a polearm or a spear, that target grants combat advantage to you until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, warden"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsWarden"/>
    </thing>
  <thing id="ftManifHea" name="Manifest Healing" description="{b}Benefit{/b}: When you use a genasi racial power, you gain 5 temporary hit points. The temporary hit points increase to 10 at 11th level and 15 at 21st level. If you have two or more elemental manifestations, the temporary hit points increase by 5." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Genasi, any primal class"/>
    <usesource source="Primal"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGenasi"/>
    <tag group="ReqPwrSrc" tag="Primal"/>
    </thing>
  <thing id="ftManifRes" name="Manifest Resistance" description="{b}Benefit{/b}: Increase the resist value provided by your elemental manifestation (if any) by 5. For example, when manifesting firesoul at 1st level to 10th level, you gain resist 10 fire (instead of resist 5 fire)." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Genasi"/>
    <usesource source="FRPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGenasi"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~fire / storm / wind: increase resist value by 5
      if (hero.child[fRGeFiresl].tagis[Helper.ChosenOpt] <> 0) then
        #traitmodify[rsFire,rsExtra,5,""]
      elseif (hero.child[fRGeStorsl].tagis[Helper.ChosenOpt] <> 0) then
        #traitmodify[rsLightn,rsExtra,5,""]
      elseif (hero.child[fRGeWindsl].tagis[Helper.ChosenOpt] <> 0) then
        #traitmodify[rsCold,rsExtra,5,""]
        endif]]></eval>
    </thing>
  <thing id="ftMantiFur" name="Manticore&apos;s Fury" description="{b}Benefit{/b}: When you hit an enemy with a melee attack, you gain a +2 bonus to the damage roll of the next ranged attack you make against that enemy before the end of your next turn. When you hit an enemy with a ranged attack, you gain a +2 bonus to the damage roll of the next melee attack you make against that enemy before the end of your next turn. This bonus increases to +4 at 11th level and +6 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ranger"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsRanger"/>
    </thing>
  <thing id="ftMantlCau" name="Mantle of Caution" description="{b}Benefit{/b}: While you are bloodied, allies within the radius of your Ardent Mantle gain a +2 bonus to Fortitude, Reflex, and Will." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, ardent, Ardent Mantle class feature"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsArdent"/>
    <prereq message="&apos;Ardent Mantle&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fArdArdMan"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMantlMis" name="Mantle of Misfortune" description="{b}Benefit{/b}: You gain mantle of misfortune as an encounter power. In addition, you gain a +2 bonus to Insight and Perception checks made against other tieflings." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="TieflingBl" name="Tiefling Bloodline"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtMantMis"></bootstrap>
    </thing>
  <thing id="ftMantlRea" name="Mantle of Readiness" description="{b}Benefit{/b}: During surprise rounds and the first non-surprise round of any encounter, you and each ally who starts his or her turn within the radius of your Ardent Mantle gain a +2 bonus to speed." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ardent, Ardent Mantle class feature"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsArdent"/>
    <prereq message="&apos;Ardent Mantle&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fArdArdMan"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMantlUnd" name="Mantle of Understanding" description="{b}Benefit{/b}: When you make a save, allies within the radius of your Ardent Mantle gain a +2 bonus to saving throws until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Ardent, Ardent Mantle class feature"/>
    <usesource source="Psionic"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsArdent"/>
    <prereq message="&apos;Ardent Mantle&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fArdArdMan"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMantlUne" name="Mantle of Unease" description="{b}Benefit{/b}: When an enemy not immune to fear scores a critical hit within the radius of your Ardent Mantle, it takes a -2 penalty to attack rolls until the end of its next turn. If you are bloodied, you also push the enemy 1 square." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Foulborn Heritage, ardent, Ardent Mantle class feature"/>
    <usesource source="Psionic"/>
    <tag group="ftDescript" tag="FoulbornBl" name="Foulborn Bloodline"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsArdent"/>
    <exprreq message="Feat &apos;Foulborn Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftFoulbHer] <> 0]]></exprreq>
    <prereq message="&apos;Ardent Mantle&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fArdArdMan"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMarkDet" name="Mark of Detection" description="{b}Benefit{/b}: When you make a Perception check, you make two rolls and use the higher result. You can also sense the presence of magic as if you were trained in Arcana. You can use Perception instead of Arcana for the check. You can master and perform rituals in the divination category as if you had the Ritual Caster feat. In addition, you can master and perform the Banish Illusions, Eavesdropper&apos;s Foil, and Scry Trap rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkEle" name="Mark of Elements" description="{b}Benefit{/b}: When you use your mark of thunder, you can choose to deal cold, fire, or lightning damage instead of thunder damage." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Primal guardian theme, mark of thunder power"/>
    <usesource source="DarkSun"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftMarkFin" name="Mark of Finding" description="{b}Benefit{/b}: When an enemy granting combat advantage to you is adjacent to you and shifts, you can shift 1 square into a square it vacates as a free action.{br}You can master and perform the Detect Object, Detect Secret Doors, Find the Path, and Inquisitive&apos;s Eyes rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkHan" name="Mark of Handling" description="{b}Benefit{/b}: You can use a creature&apos;s mount powers as if you had the Mounted Combat feat.{br}While you are mounted on a natural beast, the mount gains a +2 feat bonus to speed and a +1 feat bonus to AC.{br}If you have a beast companion, your beast companion gains a +2 feat bonus to speed and a +1 feat bonus to AC.{br}You can master and perform the Animal Messenger and Steed Summons rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkHea" name="Mark of Healing" description="{b}Benefit{/b}: Whenever you use a healing power on an ally or use Heal to allow an ally to spend his or her second wind, that ally can also make a saving throw.{br}You can master and perform rituals in the restoration category and the Remove Affliction ritual as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkHos" name="Mark of Hospitality" description="{b}Benefit{/b}: Whenever you or an ally within 10 squares of you uses a healing power during a short rest, that power restores the maximum number of hit points possible.{br}You can master and perform the Eye of Alarm, Fantastic Recuperation, Secure Shelter, and Travelers&apos; Feast  rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkMak" name="Mark of Making" description="{b}Benefit{/b}: You have mastered the Enchant Magic Item ritual and can perform it as if you were two levels higher.{br}You can make alchemical items as though you had the Alchemist feat and were two levels higher.{br}You can master and perform rituals in the creation category and the Make Whole ritual as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkPas" name="Mark of Passage" description="{b}Benefit{/b}: Whenever one of your powers lets you shift, you can shift 1 extra square.{br}Whenever one of your powers lets you teleport, you can teleport 1 extra square.{br}You can master and perform rituals in the travel category and the Enhance Vessel, Find the Path, Passwall, Phantom Steed, Steed Summons, and Water Walk rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkScr" name="Mark of Scribing" description="{b}Benefit{/b}: Choose four languages. You can speak, read, and write those languages fluently. When you gain a level, you can retrain one of these languages instead of a feat, a power, or a skill.{br}You gain a +2 bonus to Diplomacy checks.{br}You can master and perform rituals as if you had the Ritual Caster feat. Furthermore, you can create scrolls in half the normal time (that is, the same amount of time it takes create a ritual book)." compset="Feat" uniqueness="useronce">
    <comment>Bug Fixed by Dracusmage
http://hl4e.cryptoknight.org/index.php?topic=236.0</comment>
    <usesource source="EbPG"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="10000">hero.child[resLangSta].field[resMax].value += 4</eval>
    </thing>
  <thing id="ftMarkSen" name="Mark of Sentinel" description="{b}Benefit{/b}: Whenever an enemy provokes an opportunity attack from you, you can shift 1 square as a free action before or after you make the opportunity attack.{br}You can master and perform the Eye of Alarm, Eye of Warning, and Magic Circle rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkSha" name="Mark of Shadow" description="{b}Benefit{/b}: Whenever you make an attack and miss every target while hidden, you remain hidden.{br}Whenever you make an attack and miss every target while invisible, you remain invisible.{br}You can master and perform rituals in the deception and scrying categories and the Knock, Masking Shroud, Secret Page, and Shadow Walk rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkSto" name="Mark of Storm" description="{b}Benefit{/b}: Whenever you hit an enemy with a thunder or lightning power, you can slide that enemy 1 square.{br}You gain a +1 bonus to speed when flying.{br}You can master and perform the Endure Elements, Enhance Vessel, Summon Winds, and Water Walk rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkEvi" name="Mark of the Evil Eye" description="{b}Benefit{/b}: Whenever you would make an attack as the result of your Combat Challenge feature, you can use evil eye of the Vistani, if available, on the same target as a free action without provoking opportunity attacks before making your attack." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, fighter, Vistani Heritage"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="VistaniBlo" name="Vistani Bloodline"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsFighter"/>
    <exprreq message="Feat &apos;Vistani Heritage&apos; required"><![CDATA[hero.tagis[Feat.ftVistaHer] <> 0]]></exprreq>
    </thing>
  <thing id="ftMarkInf" name="Mark of the Infernal" description="{b}Benefit{/b}: The target of your infernal wrath is also subject to your divine sanction. This divine sanction lasts until the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Tiefling, paladin"/>
    <usesource source="Divine"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rTiefling"/>
    <tag group="ReqClass" tag="clsPaladin"/>
    </thing>
  <thing id="ftMarkWar" name="Mark of Warding" description="{b}Benefit{/b}: Whenever one of your powers grants a bonus to a defense, increase that bonus by 1.{br}Whenever you mark an enemy, that enemy takes a -3 penalty to attack rolls for attacks that don&apos;t target you instead of the normal -2 penalty.{br}You can master and perform rituals in the warding category and the Fluid Funds, Knock, and Leomund&apos;s Secret Chest rituals as if you had the Ritual Caster feat." compset="Feat" uniqueness="useronce">
    <usesource source="EbPG"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ftDescript" tag="Dragonmark"/>
    </thing>
  <thing id="ftMarkeDea" name="Marked for Death" description="{b}Benefit{/b}: When you deal the necrotic damage from your dark reaping racial power to a target, other enemies marked by you take necrotic damage equal to your Constitution modifier." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Con 13, revenant, ability to mark a foe"/>
    <fieldval field="reqCon" value="13"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rRevenant"/>
    </thing>
  <thing id="ftMarkeFor" name="Marked Fortunes" description="{b}Benefit{/b}: When you use Shifting Fortunes to shift, you can mark each enemy adjacent to you at the end of your shift." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Githzerai, defender role"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rGithzerai"/>
    <tag group="ReqRole" tag="Defender"/>
    </thing>
  <thing id="ftMarkeFur" name="Marked Fury" description="{b}Benefit{/b}: If an adjacent enemy that is marked by you makes an attack that doesn&apos;t include you as a target, it takes a penalty to the attack roll equal to either -2 or the number of enemies adjacent to you, whichever is worse. This penalty replaces the normal penalty for having a creature marked." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Fighter"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsFighter"/>
    </thing>
  <thing id="ftMarkeJau" name="Marked Jaunt" description="{b}Benefit{/b}: If an enemy you have marked makes an attack that doesn&apos;t include you, you can use shadow jaunt as an immediate interrupt to teleport adjacent to that enemy and then make your Combat Challenge attack, as long as that enemy is within range of shadow jaunt." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, shadar-kai, fighter, Combat Challenge class feature"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqRace" tag="rShadarkai"/>
    <tag group="ReqClass" tag="clsFighter"/>
    <prereq message="&apos;Combat Challenge&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fFtrComCha"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftMarkeSco" name="Marked Scourge" description="{b}Benefit{/b}: Once per round, you can add your Wisdom modifier to your damage roll against an enemy marked by you." compset="Feat" uniqueness="useronce">
