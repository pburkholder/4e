  <thing id="ftHellfMas" name="Hellfire Master" description="{b}Benefit{/b}: When an enemy saves against an effect bestowed by your fire attack, it takes fire damage equal to 5 + your Charisma modifier." compset="Feat" uniqueness="useronce">
