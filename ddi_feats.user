  <thing id="ftBloodLev" name="Blood of Levistus" description="{b}Benefit{/b}: You gain resistance to cold equal to 5 + one-half your level. You gain a +3 feat bonus to damage rolls with attacks that have the cold keyword." compset="Feat" uniqueness="useronce">
