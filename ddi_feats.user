  <thing id="ftSoulSor" name="Soul of Sorcery" description="{b}Benefit{/b}: Choose acid, cold, fire, force, lightning, necrotic, poison, psychic, radiant, or thunder. You gain resist 5 to that damage type.  In addition, you can wield sorcerer implements." compset="Feat">
    <fieldval field="reqText" value="Str 13, Cha 13"/>
