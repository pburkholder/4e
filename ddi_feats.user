  <thing id="ftMasteArm" name="Master at Arms" description="{b}Benefit{/b}: You gain a +1 feat bonus to the attack rolls of weapon attacks. The bonus increases to +2 at 11th level and +3 at 21st level.{br}Also, you can use a minor action to sheathe a weapon and then draw a weapon." compset="Feat" uniqueness="useronce">
    <usesource source="PlEsHeroFa"/>
