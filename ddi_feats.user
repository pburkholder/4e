  <thing id="ftCombaCom" name="Combat Commander" description="{b}Benefit{/b}: The bonus to initiative granted by your Combat Leader class feature is now equal to your Charisma modifier or your Intelligence modifier, whichever is higher." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, Warlord, Combat Leader class feature"/>
