    </thing>
  <thing id="ftSkillFoc" name="Skill Focus" description="{b}Benefit{/b}: Choose a skill in which you have training. You gain a +3 feat bonus to the chosen skill.{br}{b}Special{/b}: You can take this feat more than once. Each time you select this feat, choose a different skill." compset="Feat">
    <fieldval field="reqText" value="Training in chosen skill"/>
    <fieldval field="usrCandid1" value="component.Skill &amp; Helper.Trained?"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      perform field[usrChosen1].chosen.field[trtFeat].modify[+,3,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    </thing>
  <thing id="ftSkillPow" name="Skill Power" description="{b}Benefit{/b}: You gain a skill power of your level or lower from a skill in which you are trained." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="2nd level"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqLevel" tag="2"/>
    </thing>
  <thing id="ftSkillSwa" name="Skill Swap" description="{b}Benefit{/b}: You swap out your bonus at-will attack power for a skill power for which you meet the prerequisites. You can change this power by retraining the feat." compset="Feat" uniqueness="useronce">
