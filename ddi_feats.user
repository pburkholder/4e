    <tag group="ftDescript" tag="Divinity"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="User" tag="ReqChanDiv"/>
    <tag group="ReqDeity" tag="dtyTymora"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtTymoCoi"></bootstrap>
    </thing>
  <thing id="ftTymorSmi" name="Tymora&apos;s Smile" description="{b}Benefit{/b}: When you roll a natural 20 on an attack roll, skill check, or saving throw, you and one ally within 5 squares of you can make a saving throw." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftTyriaAsc" name="Tyrian Ascendant" description="{b}Benefit{/b}: In addition to regaining the use of your fell might, whenever you drop an enemy you have cursed to 0 hit points, each enemy cursed by you takes a -2 penalty to attack rolls until the end of your next turn.{br}You can also expend your fell might to augment any power associated with this feat. When you hit an enemy with an associated power augmented by this feat, you also knock the target prone.{br}Finally, you can use Charisma instead of Constitution for attack rolls and damage rolls with any power associated with this feat." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, Sorcerer-King Pact class feature, Tyrian Bureaucrat feat"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="GreaterPac" name="Greater Pact"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <exprreq message="Feat &apos;Tyrian Bureaucrat&apos; required"><![CDATA[hero.tagis[Feat.ftTyriaBur] <> 0]]></exprreq>
    <prereq message="&apos;Sorcerer-King Pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkSorPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftTyriaBur" name="Tyrian Bureaucrat" description="{b}Benefit{/b}: You gain a +2 feat bonus to Streetwise checks.{br}When you augment hand of blight, you can forgo the extra damage the augment would deal to instead daze the target until the end of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Warlock, Sorcerer-King Pact class feature"/>
    <usesource source="Dragon"/>
    <tag group="ftDescript" tag="LesserPact" name="Lesser Pact"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsWarlock"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[skStreetwi,trtFeat,2,""]]]>
      <before name="Derived trtFinal"/>
      </eval>
    <prereq message="&apos;Sorcerer-King Pact&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fWlkSorPac"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftTyriaVic" name="Tyrian Victory Practice" description="{b}Benefit{/b}: You gain a +2 feat bonus to Heal checks.{br}When you use a power associated with this feat and hit an enemy with it, you gain a +2 bonus to the next saving throw you make before the start of your next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class"/>
    <usesource source="DarkSun"/>
    <tag group="ftDescript" tag="ArenaFight" name="Arena Fighting"/>
    <tag group="Tier" tag="Heroic"/>
