    <tag group="ReqClass" tag="clsBard"/>
    <prereq message="&apos;Majestic word&apos; feature required">
      <validate>perform hero.childfound[pBrdMajWor].setfocus
call ReqPower</validate>
      </prereq>
    </thing>
  <thing id="ftBarreCha" name="Barreling Charge" description="{b}Benefit{/b}: When you charge with a reach weapon, you can end your charge in the nearest square adjacent to the target of the charge." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Any martial class"/>
    <usesource source="Martial2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqPwrSrc" tag="Martial"/>
    </thing>
  <thing id="ftBatteShi" name="Battering Shield" description="{b}Benefit{/b}: When you are wielding a heavy shield and push or slide a target with a melee attack, you can move that target 1 additional square." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Proficiency with heavy shields"/>
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftBattlAcu" name="Battle Acumen" description="{b}Benefit{/b}: You gain the fighter&apos;s Combat Superiority class feature." compset="Feat" uniqueness="useronce">
