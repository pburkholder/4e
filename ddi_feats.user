    <tag group="Hide" tag="Special"/>
    <bootstrap thing="pFtSmalVen"></bootstrap>
    </thing>
  <thing id="ftSmallWar" name="Small Warrior&apos;s Defense" description="{b}Benefit{/b}: While you are using a versatile weapon in both hands, you gain a +2 bonus to AC and Reflex." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Fighter, Small size"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsFighter"/>
    <eval phase="Setup" priority="1000"><![CDATA[
      ~check if we're wielding a versatile weapon in both hands - if not, do
      ~nothing
      doneif (hero.haschild[WeapMelee,"WepProp.Versatile & Helper.EqpBoth"] = 0)

      ~+2 to reflex and AC
      #traitmodify[defAC,trtBonus,2,""]
      #traitmodify[defRef,trtBonus,2,""]]]></eval>
    </thing>
  <thing id="ftSnakeBlo" name="Snake Blooded" description="{b}Benefit{/b}: You gain resist 5 poison. In addition, you gain a +2 feat bonus to saving throws against poison effects." compset="Feat" uniqueness="useronce">
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      #traitmodify[rsPoison,rsTotal,5,""]]]></eval>
    </thing>
  <thing id="ftSnappLiz" name="Snapping Lizard" description="{b}Benefit{/b}: Your beast companion gains a +2 bonus to saving throws.{br}While your beast companion acts independently, it need not move adjacent to you but must remain within 10 squares of you.{br}In addition, if it hits a creature with a melee basic attack, that creature takes 5 damage if it moves away from the beast companion before the end of the creature&apos;s next turn." compset="Feat" uniqueness="useronce">
