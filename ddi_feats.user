  <thing id="ftRuneSSou" name="Rune-Scribed Soul" description="{b}Benefit{/b}: When you use your second wind, you gain a +1 bonus to attack rolls and a bonus to damage equal to your Constitution modifier with arcane powers you use before the end of your turn." compset="Feat" uniqueness="useronce">
