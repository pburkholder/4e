      foreach pick in hero from WeaponBase where "(thingid.wpDagger | thingid.wpShuriken)"
        if (eachpick.tagis[WepProp.HighCrit] = 0) then
          perform eachpick.assign[WepProp.HighCrit]
          endif
        nexteach]]></eval>
