    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRace" tag="rDrow"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      foreach pick in hero from WeaponBase where "(thingid.wpHandCro)"
        perform eachpick.delete[WepDamage.?]
        perform eachpick.assign[WepDamage.1d8]
        if (eachpick.tagis[WepProp.HighCrit] = 0) then
          perform eachpick.assign[WepProp.HighCrit]
          endif
        nexteach]]></eval>
    </thing>
  <thing id="ftRuthlInj" name="Ruthless Injury" description="{b}Benefit{/b}: When you use a club or a mace to make a sneak attack that causes the target to become blinded, immobilized, slowed, or weakened, that target takes a -2 penalty to saving throws against any of those conditions." compset="Feat" uniqueness="useronce">
