    <exprreq message="Requires Warlord multiclass"><![CDATA[hero.tagis[Multiclass.clsWarlord] <> 0]]></exprreq>
    </thing>
  <thing id="ftBattlInt" name="Battle Intuition" description="{b}Benefit{/b}: You can use your Wisdom modifier in place of your Dexterity modifier to determine your initiative bonus. Additionally, you gain a +2 feat bonus to initiative checks." compset="Feat" uniqueness="useronce">
