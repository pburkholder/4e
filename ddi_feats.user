      ~resist many 2/3/5
      var bonus as number
      if (#level[] <= 10) then
        bonus = 5
      elseif (#level[] <= 20) then
        bonus = 10
      else
        bonus = 15
        endif
      #traitmodify[rsCold,rsTotal,bonus,""]]]></eval>
