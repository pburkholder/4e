  <thing id="ftMeleeTra" name="Melee Training" description="{b}Benefit{/b}: Choose an ability other than Strength. When you make a melee basic attack using a weapon with which you have proficiency, you can use the chosen ability instead of Strength for the attack roll. In addition, you can use half of that ability&apos;s modifier, instead of your Strength modifier, for the damage roll." compset="Feat" uniqueness="useronce">
    <arrayval field="usrArray" index="0" value="Constitution"/>
    <arrayval field="usrArray" index="1" value="Dexterity"/>
    <arrayval field="usrArray" index="2" value="Intelligence"/>
    <arrayval field="usrArray" index="3" value="Wisdom"/>
    <arrayval field="usrArray" index="4" value="Charisma"/>
    <usesource source="PHB2"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <eval phase="Traits" priority="10000"><![CDATA[perform hero.child[pwBasicMel].delete[Attack.attrStr]
perform hero.child[pwBasicMel].delete[DamageAttr.attrStr]

var DamMod as number



if (compare(field[usrSelect].text,"Constitution" ) = 0) then
perform hero.child[pwBasicMel].assign[Attack.attrCon]
perform hero.child[pwBasicMel].assign[DamageAttr.attrCon]
DamMod=#attrbasebonus[attrCon]


elseif (compare(field[usrSelect].text,"Charisma" ) = 0) then
perform hero.child[pwBasicMel].assign[Attack.attrCha]
perform hero.child[pwBasicMel].assign[DamageAttr.attrCha]
DamMod=#attrbasebonus[attrCha]

elseif (compare(field[usrSelect].text, "Dexterity") = 0) then
perform hero.child[pwBasicMel].assign[Attack.attrDex]
perform hero.child[pwBasicMel].assign[DamageAttr.attrDex]
DamMod=#attrbasebonus[attrDex]

elseif (compare(field[usrSelect].text, "Intelligence") = 0) then
perform hero.child[pwBasicMel].assign[Attack.attrInt]
perform hero.child[pwBasicMel].assign[DamageAttr.attrInt]
DamMod=#attrbasebonus[attrInt]

elseif (compare(field[usrSelect].text, "Wisdom") = 0) then
perform hero.child[pwBasicMel].assign[Attack.attrWis]
perform hero.child[pwBasicMel].assign[DamageAttr.attrWis]
DamMod=#attrbasebonus[attrWis]
endif

DamMod=int(DamMod/2+.5)
DamMod=DamMod*-1

perform hero.child[pwBasicMel].field[pwDamBonus].modify[+,DamMod,""]]]></eval>
    </thing>
  <thing id="ftMelorTid" name="Melora&apos;s Tide" description="{b}Benefit{/b}: You gain the Melora&apos;s tide power." compset="Feat" uniqueness="useronce">
