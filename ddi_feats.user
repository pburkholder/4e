    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsSorcere"/>
    <prereq message="&apos;Wild Magic&apos; feature required">
      <validate><![CDATA[var featureid as string
featureid = "fSorWilMag"
call ReqFeature]]></validate>
      </prereq>
    </thing>
  <thing id="ftGamblWor" name="Gambler&apos;s Word" description="{b}Benefit{/b}: Once per encounter, when you score a critical hit, you gain an additional use of your healing word power during this encounter." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="11th level, cleric, healing word power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqLevel" tag="11"/>
    <tag group="ReqClass" tag="clsCleric"/>
    </thing>
  <thing id="ftGarroExp" name="Garrote Expert" description="{b}Benefit{/b}: You can swap one 6th-level or higher utility power you know for the body shield (garrote expert) utility power." compset="Feat" uniqueness="useronce">
