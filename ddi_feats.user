    <tag group="Multiclass" tag="clsFighter"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="fFtrComSup"></bootstrap>
    <exprreq message="Requires Fighter multiclass"><![CDATA[hero.tagis[Multiclass.clsFighter] <> 0]]></exprreq>
    </thing>
  <thing id="ftBattlAwa" name="Battle Awareness" description="{b}Benefit{/b}: You gain training in one skill from the fighter&apos;s class skill list.{br}Once per encounter, whenever an enemy that is adjacent to you shifts or makes an attack that does not include you as a target, you can make a melee basic attack against that enemy as an immediate interrupt." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 13, Wis 13"/>
    <fieldval field="reqStr" value="13"/>
    <fieldval field="reqWis" value="13"/>
    <fieldval field="usrCandid1" value="component.Skill &amp; (thingid.skAthletic | thingid.skEnduranc | thingid.skHeal | thingid.skIntimida | thingid.skStreetwi)"/>
    <usesource source="Martial"/>
    <tag group="Multiclass" tag="clsFighter"/>
    <tag group="Hide" tag="Special"/>
    <tag group="Tier" tag="Heroic"/>
    <bootstrap thing="pFtBattAwa"></bootstrap>
    <eval phase="Traits" priority="1000">perform field[usrChosen1].chosen.assign[Helper.TrainedAut]
      <before name="Calc sklTrained"/>
      </eval>
    </thing>
  <thing id="ftBattlBer" name="Battle Berserker" description="{b}Benefit{/b}: You gain training in a skill of your choice from the barbarian&apos;s list of class skills.{br}You also gain the berserker barbarian&apos;s Berserker Fury feature, but you can use it only once per day." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Strength 13"/>
    <fieldval field="reqStr" value="13"/>
    <usesource source="HeroFeywil"/>
    <tag group="Multiclass" tag="clsBarbari"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftBattlCad" name="Battle Cadence" description="{b}Benefit{/b}: When you use majestic word, you can slide the target 1 extra square. The target also gains a +2 bonus to the next damage roll he or she makes before the end of his or her next turn." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Bard, majestic word power"/>
    <usesource source="Dragon"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBard"/>
    </thing>
  <thing id="ftBattlCas" name="Battle Caster Defense" description="{b}Benefit{/b}: You gain a +4 bonus to AC against opportunity attacks that you provoke when using a ranged or an area power." compset="Feat" uniqueness="useronce">
    <usesource source="PHB3"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftBattlCle" name="Battle Cleric Armaments" description="{b}Benefit{/b}: You gain proficiency with light shields and with one military weapon of your choice." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Str 15, cleric"/>
    <fieldval field="reqStr" value="15"/>
    <fieldval field="usrCandid1" value="component.WeaponBase &amp; !Equipment.Natural &amp; !component.MagicItem &amp; !thingid.wpMagic? &amp; WepCat.wcMilitary"/>
