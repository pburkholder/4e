    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqClass" tag="clsBarbari"/>
    </thing>
  <thing id="ftRaginDra" name="Raging Drake Form" description="{b}Benefit{/b}: While you are in beast form, you gain a +1 bonus to attack rolls and damage rolls while bloodied." compset="Feat" uniqueness="useronce">
