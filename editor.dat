<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">


  <editthing
    compset="Attribute"
    name="Attribute"
    prefix="attr"
    description="Attributes are the core abilities of a character - strength, dexterity, etc."
    summary="Defines an Attribute that each hero has.">
    <inputthing
      name="Starting Value"
      helptext="Specify the starting value for the attribute. If not specified, defaults to 10.">
      <it_field field="trtUser"/>
      </inputthing>
    <inputthing
      name="Sort Order"
      helptext="Specify the sort order of the attribute. Strength has a sort order of 5, Constitution 10, Dexterity 15, Intelligence 20, Wisdom 25, and Charisma 30.{br}{br}For example, if you set this value to &quot;{b}17{/b}&quot;, the new attribute would appear between Dexterity and Intelligence.">
      <it_tagpick group="explicit" tag="?"/>
      </inputthing>
    <inputthing
      name="Physical Attribute?"
      helptext="Check whether this attribute is a physical attribute.">
      <it_tagcheck group="User" tag="AttrPhys"/>
      </inputthing>
    <inputthing
      name="Mental Attribute?"
      helptext="Check whether this attribute is a mental attribute.">
      <it_tagcheck group="User" tag="AttrMental"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Alignment"
    name="Alignment"
    prefix="al"
    description="A hero's alignment defines his approach to life."
    summary="Create new alignments that can be chosen by the hero.">
    <inputthing
      name="Sort Order"
      helptext="Specify the sort order of the alignment. Lawful Good has a sort order of 10, Good 20, Neutral 30, Evil 40, Chaotic Evil 50.{br}{br}For example, if you set this value to &quot;{b}17{/b}&quot;, the new alignment would appear between Lawful Good and Good.">
      <it_tagpick group="explicit" tag="?"/>
      </inputthing>
    <inputthing
      name="Alignment Tags"
      helptext="Click here to choose tags for the alignment.">
      <it_taglist group="Alignment" tag="?"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Defense"
    name="Defense"
    prefix="def"
    description="Defenses are the traits the hero uses to resist damage - Armor Class, Fortitude, Will, etc."
    summary="Defines a Defense that each hero has.">
    <inputthing
      name="Defense Abbreviation"
      helptext="Specify a short abbreviation for the defense. For example, &quot;Fort&quot; or &quot;Ref&quot;.">
      <it_field field="trtAbbrev"/>
      </inputthing>
    <inputthing
      name="Starting Value"
      helptext="Specify the default value for the defense. If not specified, defaults to 0.">
      <it_field field="trtBonus"/>
      </inputthing>
    <inputthing
      name="Sort Order"
      helptext="Specify the sort order of the defense. Armor Class has a sort order of 5, Fortitude 10, Reflex 15 and Will 20.{br}{br}For example, if you set this value to &quot;{b}17{/b}&quot;, the new defense would appear between Reflex and Will.">
      <it_tagpick group="explicit" tag="?"/>
      </inputthing>
    <inputthing
      name="First Linked Attribute"
      helptext="Select the first attribute linked to this defense. The best of these two attributes is added as a bonus to the defense.">
      <it_linkage compset="Attribute" linkage="defAttr1"/>
      </inputthing>
    <inputthing
      name="Second Linked Attribute"
      helptext="Select the second attribute linked to this defense. The best of these two attributes is added as a bonus to the defense.">
      <it_linkage compset="Attribute" linkage="defAttr2"/>
      </inputthing>
    <inputthing
      name="No Attribute Bonus in Heavy Armor?"
      helptext="Check if this defense receives no attribute bonus while wearing Heavy Armor (e.g. Armor Class).">
      <it_tagcheck group="User" tag="NoAttHeavy"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Trait"
    name="Trait"
    prefix="trt"
    description="Traits appear in the Abilities summary panel and sometimes on the Abilities tab."
    summary="Traits track quantities like initiative values, senses, size, etc.">
    <inputthing
      name="Trait Abbreviation"
      helptext="Specify a short abbreviation for the trait. For example, &quot;Lvl&quot; or &quot;Init&quot;.">
      <it_field field="trtAbbrev"/>
      </inputthing>
    <inputthing
      name="Starting Value"
      helptext="Specify the default value for the trait. If not specified, defaults to 0.">
      <it_field field="trtBonus"/>
      </inputthing>
    <inputthing
      name="Sort Order"
      helptext="Specify the sort order of the trait within the Abilities tab and summary panel.">
      <it_tagpick group="explicit" tag="?"/>
      </inputthing>
    <inputthing
      name="Appears in Narrow Traits Table?"
      helptext="Check if this trait appears in the narrow traits table (containing level, initiative and skills) on the Abilities tab.">
      <it_tagcheck group="User" tag="BasicsNarr"/>
      </inputthing>
    <inputthing
      name="Appears in Wide Traits Table?"
      helptext="Check if this trait appears in the wide traits table (containing size, senses and hit points) on the Abilities tab.">
      <it_tagcheck group="User" tag="BasicsWide"/>
      </inputthing>
    <inputthing
      name="Appears in Tactical Con.?"
      helptext="Specify if this trait appears on the Dashboard or Tactical Console.">
      <it_tagpick group="DashTacCon" tag="?"/>
      </inputthing>
    <inputthing
      name="Adjustments allowed?"
      helptext="Check if this trait can be changed by adding an in-play adjustment on the In-Play tab.">
      <it_tagcheck group="User" tag="CanAdjust"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Resistance"
    name="Resist"
    prefix="rs"
    description="Resistance to a type of attack, such as Fire or Radiant damage."
    summary="Resistance to a type of attack, such as Fire or Radiant damage.">
    </editthing>


  <editthing
    compset="Skill"
    name="Skill"
    prefix="sk"
    description="Basic skills that can be used by the hero in skill checks."
    summary="Defines a Skill which can be used by the hero.">
    <inputthing
      name="Linked Attribute"
      helptext="Select the attribute that gives a bonus to this skill.">
      <it_linkage compset="Attribute" linkage="attribute"/>
      </inputthing>
    <inputthing
      name="Passive Sense?"
      helptext="Check if this skill should be included on the Passive Senses table on the printout like Insight and Perception.">
      <it_tagcheck group="User" tag="PassSense"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Feat"
    name="Feat"
    prefix="ft"
    description="Feats can assign a hero diverse special abilities."
    summary="Defines a Feat which can be chosen from the Feats tab.">
    <inputthing
      name="Feat Use"
      helptext="If this feat can be used like a power, specify how it can be used.">
      <it_tagpick group="PowerUse" tag="?"/>
      </inputthing>
    <inputthing
      name="Hide from Special Tab?"
      helptext="Specify whether this feat is not displayed on the Special tab. Typically this is done with feats that assign a fixed bonus to your statistics, since their effects are already built in to your hero.">
      <it_tagcheck group="Hide" tag="Special"/>
      </inputthing>
    <inputthing
      name="Descriptors"
      helptext="Click here to choose descriptors for this feat.">
      <it_taglist group="ftDescript" tag="?"/>
      </inputthing>
    <inputthing
      name="Multiclass Feat?"
      helptext="If this feat is a multiclass feat, pick the character class it grants multiclassing into.">
      <it_tagpick group="Multiclass" tag="?"/>
      </inputthing>

    <inputthing
      name="Selection List 1"
      helptext="Many feats require you to select from lists of things, which can be configured here. You can select from a fixed list, a list of things, or have a checkbox.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="List of Text Items"
      helptext="A list of items that can be chosen from by the feat. Enter one item per row.">
      <it_field field="usrArray"/>
      </inputthing>
    <inputthing
      name="Initial Item"
      helptext="Select the initial item to be selected from the list. If left blank, the first item will be selected by default.">
      <it_field field="usrSelect"/>
      </inputthing>
    <inputthing
      name="Tag Expression"
      helptext="To allow selection of a thing (for example, a skill, ability score or weapon) from a menu, enter a tag expression.">
      <it_field field="usrCandid1"/>
      </inputthing>
    <inputthing
      name="Select From..."
      helptext="Choose which list to select items from. You can choose All Things (everything, even those not added to the hero); All Picks on Hero (the default) or All Picks on Container (which may be useful on gizmos sometimes).">
      <it_tagpick group="ChooseSrc1" tag="?"/>
      </inputthing>
    <inputthing
      name="Checkbox Text"
      helptext="To show a checkbox that can be selected by the user, enter the label text for the checkbox here.">
      <it_field field="usrChkText"/>
      </inputthing>

    <inputthing
      name="Selection List 2"
      helptext="Sometimes a second list is required, which can be selected here.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Tag Expression"
      helptext="To allow selection of a thing (for example, a skill, ability score or weapon) from a menu, enter a tag expression.">
      <it_field field="usrCandid2"/>
      </inputthing>
    <inputthing
      name="Select From..."
      helptext="Choose which list to select items from. You can choose All Things (everything, even those not added to the hero); All Picks on Hero (the default) or All Picks on Container (which may be useful on gizmos sometimes).">
      <it_tagpick group="ChooseSrc2" tag="?"/>
      </inputthing>

    <inputthing
      name="Requirements"
      helptext="Lists any requirements that you must meet before the feat can be selected.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Tier"
      helptext="Pick the tier this feat requires.">
      <it_tagpick group="Tier" tag="?" deftag="Heroic" require="yes"/>
      </inputthing>
    <inputthing
      name="Requirement Details"
      helptext="Text that identifies exactly what the feat requires.">
      <it_field field="reqText"/>
      </inputthing>
    <inputthing
      name="Race Requirement"
      helptext="Choose a race the feat requires, if any.">
      <it_taglist group="ReqRace" tag="?"/>
      </inputthing>
    <inputthing
      name="Class Requirement"
      helptext="Choose a class the feat requires, if any.">
      <it_taglist group="ReqClass" tag="?"/>
      </inputthing>
    <inputthing
      name="Level Requirement"
      helptext="Choose a level the feat requires, if any.">
      <it_tagpick group="ReqLevel" tag="?"/>
      </inputthing>
    <inputthing
      name="Strength Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqStr"/>
      </inputthing>
    <inputthing
      name="Constitution Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqCon"/>
      </inputthing>
    <inputthing
      name="Dexterity Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqDex"/>
      </inputthing>
    <inputthing
      name="Intelligence Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqInt"/>
      </inputthing>
    <inputthing
      name="Wisdom Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqWis"/>
      </inputthing>
    <inputthing
      name="Charisma Required"
      helptext="The hero must meet this minimum ability score requirement to pick the feat.">
      <it_field field="reqCha"/>
      </inputthing>
    <inputthing
      name="Trained Skill Requirement"
      helptext="Choose a skill the hero must be trained in to take the feat.">
      <it_tagpick group="ReqSkill" tag="?"/>
      </inputthing>
    <inputthing
      name="Deity Requirement"
      helptext="Choose which deity the feat requires, if any.">
      <it_tagpick group="ReqDeity" tag="?"/>
      </inputthing>
    <inputthing
      name="Weapon Proficiency Requirement"
      helptext="Choose which weapon proficiencies the feat requires, if any.">
      <it_tagpick group="ReqWepProf" tag="?"/>
      </inputthing>
    <inputthing
      name="Role Requirement"
      helptext="Choose a class role the hero must meet to take the feat.">
      <it_tagpick group="ReqRole" tag="?"/>
      </inputthing>
    <inputthing
      name="Power Source Requirement"
      helptext="Choose a power source that must be available for the hero to take the feat.">
      <it_tagpick group="ReqPwrSrc" tag="?"/>
      </inputthing>
    <inputthing
      name="Channel Divinity required?"
      helptext="Check whether the Channel Divinity power is required to take this feat.">
      <it_tagcheck group="User" tag="ReqChanDiv"/>
      </inputthing>
    </editthing>


  <editthing
    compset="Race"
    name="Race"
    prefix="r"
    description="Races can give bonuses to attributes, defenses and traits, as well as defining the physical characteristics of the hero."
    summary="Defines a Race that can be selected by a character.">
    <inputthing
      name="Flavor Text"
      helptext="Flavor text that describes the race.">
      <it_field field="racFlavor"/>
      </inputthing>

    <inputthing
      name="Ability Scores &amp; Defenses"
      helptext="Changes to ability scores and defenses made by the race.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Strength Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racStr"/>
      </inputthing>
    <inputthing
      name="Constitution Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racCon"/>
      </inputthing>
    <inputthing
      name="Dexterity Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racDex"/>
      </inputthing>
    <inputthing
      name="Intelligence Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racInt"/>
      </inputthing>
    <inputthing
      name="Wisdom Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racWis"/>
      </inputthing>
    <inputthing
      name="Charisma Bonus"
      helptext="A bonus to an ability score assigned by the race.">
      <it_field field="racCha"/>
      </inputthing>
    <inputthing
      name="AC Bonus"
      helptext="A bonus to defense assigned by the race.">
      <it_field field="racAC"/>
      </inputthing>
    <inputthing
      name="Fortitude Bonus"
      helptext="A bonus to defense assigned by the race.">
      <it_field field="racFort"/>
      </inputthing>
    <inputthing
      name="Reflex Bonus"
      helptext="A bonus to defense assigned by the race.">
      <it_field field="racRef"/>
      </inputthing>
    <inputthing
      name="Will Bonus"
      helptext="A bonus to defense assigned by the race.">
      <it_field field="racWill"/>
      </inputthing>
    <inputthing
      name="Ability Bonus Choices"
      helptext="Add one or more ability bonus choices that the characters of this race can pick.">
      <it_bootcustom compset="RaceFeat">
        <match>thingid.fRPlusAb</match>
        <inputthing
          name="Attributes"
          helptext="Pick the attributes that can be chosen for this bonus.">
          <it_taglist group="RaceBonus" tag="?" dynamic="no"/>
          </inputthing>
        </it_bootcustom>
      </inputthing>
    <inputthing
      name="Other Traits"
      helptext="Miscellaneous changes made by the race.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Size"
      helptext="Pick the size of members of this race.">
      <it_tagpick group="Size" tag="?" deftag="Medium" require="yes"/>
      </inputthing>
    <inputthing
      name="Speed (squares)"
      helptext="The races default speed. If empty, it is set to 6.">
      <it_field field="racSpeed"/>
      </inputthing>
    <inputthing
      name="Initiative Bonus"
      helptext="A bonus to initiative assigned by the race.">
      <it_field field="racInit"/>
      </inputthing>
    <inputthing
      name="Skill Bonuses"
      helptext="Pick skills to which this race assigns a +2 bonus.">
      <it_taglist group="SkillBonus" tag="?"/>
      </inputthing>
    <inputthing
      name="Bonus Trained Skills"
      helptext="The number of extra trained skills the race allows you.">
      <it_field field="racSkill"/>
      </inputthing>
    <inputthing
      name="Weapon Proficiencies"
      helptext="Pick free weapon proficiencies gained by this race.">
      <it_taglist group="WeaponProf" tag="?"/>
      </inputthing>
    <inputthing
      name="Counts As Races"
      helptext="If this race counts as any others (for example, half-elves counting as Humans and Elves) select them here.">
      <it_taglist group="Race" tag="?"/>
      </inputthing>
    <inputthing
      name="Free Languages"
      helptext="Select the free languages learned by the race here.">
      <it_bootlist compset="Language"/>
      </inputthing>
    <inputthing
      name="Bonus Languages"
      helptext="The number of extra languages the race allows you, on top of the free languages it assigns.">
      <it_field field="racLang"/>
      </inputthing>
    <inputthing
      name="Standard Race?"
      helptext="If checked, this race is disabled when the 'Hide Standard Races' setting is selected on the Configure Hero form.">
      <it_tagcheck group="User" tag="StdRace" default="yes"/>
      </inputthing>

    <inputthing
      name="Height &amp; Weight"
      helptext="The minimum and maximum height and weight of the race.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Minimum Height"
      helptext="Minimum height (in inches).">
      <it_field field="racHtMin"/>
      </inputthing>
    <inputthing
      name="Maximum Height"
      helptext="Maximum height (in inches).">
      <it_field field="racHtMax"/>
      </inputthing>
    <inputthing
      name="Minimum Weight"
      helptext="Minimum weight (in pounds).">
      <it_field field="racWtMin"/>
      </inputthing>
    <inputthing
      name="Maximum Weight"
      helptext="Maximum weight (in pounds).">
      <it_field field="racWtMax"/>
      </inputthing>
    </editthing>


