    <tag group="ReqLevel" tag="18"/>
    <tag group="RitualCat" tag="Travel"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtPortaJum" name="Portal Jump" description="Choose one unoccupied square adjacent to you and one unoccupied square within 20 squares of you. You create a teleportation portal to link those squares. Any creature that enters a linked square can teleport to the other square by using 1 square of movement. Whenever a creature teleports in this way, it is dazed until the end of its next turn.{br}Ritual Scroll: You can perform this ritual as a standard action when you do so from a scroll. If you do, the portal lasts until the end of your next turn." compset="Ritual">
    <fieldval field="ritCompCst" value="75 gp and 1 healing surge"/>
    <fieldval field="ritTime" value="1 minute"/>
    <fieldval field="ritLasts" value="2 rounds"/>
    <fieldval field="ritPrice" value="360"/>
