    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="3"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtSundeSki" name="Sundered Skies" description="You must perform this ritual in an outdoor, aboveground environment. The ritual gathers clouds overhead in a 10-square radius centered on you. When you finish the ritual, precipitation (rain, or snow if the temperature is below freezing) pours from the clouds for the ritual&apos;s duration. This magical precipitation moves with you, remaining centered above your space.{br}The magical precipitation extinguishes exposed nonmagical flames in the area instantly and lightly obscures all squares in the area. Creatures in the area gain a +2 power bonus to saving throws against ongoing fire damage.{br}If you go indoors, the ritual is discharged. You can also take a minor action to end the ritual&apos;s effect. In either case, the gathered clouds disappear over the course of 1 minute." compset="Ritual">
    <fieldval field="ritCompCst" value="125 gp"/>
    <fieldval field="ritTime" value="1 minute"/>
    <fieldval field="ritLasts" value="10 minutes or until discharged"/>
    <fieldval field="ritPrice" value="500"/>
    <usesource source="Dragon"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="7"/>
    <tag group="RitualSkl" tag="skNature"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtSupprCry" name="Suppression Crystal" description="A suppression crystal catches sound and energy, trapping them and safely bleeding them." compset="Ritual">
    <fieldval field="ritCompCst" value="See below"/>
    <fieldval field="ritTime" value="1 hour"/>
    <fieldval field="ritPrice" value="120"/>
    <usesource source="EbPG"/>
