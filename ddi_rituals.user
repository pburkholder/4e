    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtStoneSha" name="Stone Shape" description="You form a volume of stone, up to 10 feet on a side, into whatever shape you wish. You could sculpt a statue, reshape a stone door, open a hole in a wall, and so on. You can manage only crude details, so you cannot create objects that have moving parts." compset="Ritual">
    <fieldval field="ritCompCst" value="150 gp and 1 healing surge"/>
    <fieldval field="ritTime" value="1 minute"/>
    <fieldval field="ritLasts" value="Instantaneous"/>
    <fieldval field="ritPrice" value="680"/>
    <usesource source="Dragon"/>
    <tag group="RitualSkl" tag="skNature"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="8"/>
