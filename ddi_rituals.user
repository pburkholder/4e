    <tag group="ReqLevel" tag="4"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtElemeTra" name="Elemental Transference" description="Use this ritual in the world. When you perform this ritual, you transport yourself and up to eight allies to a random spot in the Elemental Chaos. You have no control over where you appear, but your immediate environs aren&apos;t hazardous. Once in the Elemental Chaos, you can travel as normal within the plane. You remain in the Elemental Chaos until you leave by another means." compset="Ritual">
    <fieldval field="ritCompCst" value="1,000 gp"/>
    <fieldval field="ritTime" value="1 hour"/>
    <fieldval field="ritLasts" value="Special"/>
    <fieldval field="ritPrice" value="2600"/>
    <usesource source="ManPlanes"/>
    <tag group="RitualCat" tag="Travel"/>
    <tag group="ReqLevel" tag="12"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtEmbalm" name="Embalm" description="You use this martial practice on an adjacent corpse. The practice doubles the time a corpse can be dead and still be affected by Raise Dead or a similar ritual." compset="Ritual">
