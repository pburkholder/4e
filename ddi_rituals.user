    <tag group="RitualCat" tag="MartialPra" name="Martial Practice"/>
    <tag group="ReqLevel" tag="3"/>
    <tag group="RitualSkl" tag="skNature"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtWaterBre" name="Water Breathing" description="When you perform this ritual, you can designate up to eight participants (including yourself if you wish) who breathe water as easily as they breathe air. Furthermore, they can speak normally underwater.{br}Water Breathing doesn&apos;t change your ability to breathe air. It doesn&apos;t confer a greater swim speed or the ability to fight underwater unhindered. It does protect any gear you and the participants carry, such as books and scrolls, from water damage.{br}Your check result determines the duration of the effect.{br}{br}Arcana Check Result - Duration{br}14 or lower - 1 hour{br}15-19 - 2 hours{br}20-24 - 4 hours{br}25-29 - 8 hours{br}30 or higher - 24 hours" compset="Ritual">
    <fieldval field="ritCompCst" value="135 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="Special"/>
    <fieldval field="ritPrice" value="680"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="8"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="skNature"/>
    </thing>
  <thing id="rtWaterWal" name="Water Walk" description="You or an ally can move on water as if it were solid ground. Rapids or choppy seas are considered difficult terrain. The target of this ritual can end its benefits as a free action. The target can immerse itself in water or swim underwater if desired without needing to end the ritual." compset="Ritual">
    <fieldval field="ritCompCst" value="20 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="1 hours"/>
    <fieldval field="ritPrice" value="100"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="2"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    <tag group="RitualSkl" tag="skNature"/>
    </thing>
  <thing id="rtWaterGif" name="Water&apos;s Gift" description="You and up to seven other creatures present for the ritual take on certain aquatic abilities: the ability to breathe water as easily as air, to speak underwater normally, and to fight underwater without penalty. Each subject also gains a +5 power bonus to Athletics checks to swim. {br}{br}Your Nature check result determines the duration of the effect.{br}{br}Nature Check Result - Duration{br}14 or lower - 3 hours{br}15-24 - 6 hours{br}25 or higher - 12 hours" compset="Ritual">
    <fieldval field="ritCompCst" value="400 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="Special"/>
    <fieldval field="ritPrice" value="1000"/>
    <usesource source="Primal"/>
    <tag group="ReqLevel" tag="10"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="RitualSkl" tag="skNature"/>
    </thing>
  <thing id="rtWaterbor" name="Waterborn" description="Targets of the Waterborn ritual can explore the ocean&apos;s depths at length and with leisure. Up to eight creatures (including yourself if you choose) gain the ability to breathe underwater. In addition to this ability, targets can speak normally and gain a swim speed equal to their land speed. Subjects are unaffected by the cold and pressure of the deep ocean.{br}This ritual does not negate the ability to breathe air or move on land. Equipment and possessions, such as books and scrolls, on a creature affected by the ritual remain undamaged by the water.{br}{br}Check Result - Duration{br}19 or lower - 4 hours{br}20-29 - 8 hours{br}30-39 - 16 hours{br}40 or higher - 2 days" compset="Ritual">
    <fieldval field="ritCompCst" value="850 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="Special"/>
    <fieldval field="ritPrice" value="2000"/>
    <usesource source="FRPG"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="ReqLevel" tag="14"/>
    <tag group="RitualSkl" tag="skNature"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtWavesEnc" name="Wavestrider Enchantment" description="You and each ally within 2 squares of you can move on water as if it were solid ground. Rapids or choppy seas are considered difficult terrain. The target of this ritual can end its benefit as a free action. The target can immerse itself in water or swim underwater if desired without needing to end the ritual." compset="Ritual">
    <fieldval field="ritCompCst" value="Focus (wavestrider boots)"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="1 hour"/>
    <fieldval field="ritPrice" value="175"/>
    <usesource source="Dragon"/>
    <tag group="ReqLevel" tag="4"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtWhispEdi" name="Whispers of the Edifice" description="You ask questions of and receive verbal answers from a single room or area. Your Arcana check result determines the number of questions you can ask. The room or area can be no larger than your level in squares on a side. The room knows what has happened inside it for its entire existence, but it is incapable of making any judgments or analysis.{br}{br}Arcana Check Result - Questions Allowed{br}9 or lower - None{br}10-19 - 1 question{br}20-29 - 2 questions{br}30-39 - 3 questions{br}40 or higher - 4 questions" compset="Ritual">
    <fieldval field="ritCompCst" value="800 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="5 minutes"/>
    <fieldval field="ritPrice" value="4200"/>
    <usesource source="Arcane"/>
    <tag group="ReqLevel" tag="14"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtWindWor" name="Wind Words" description="You whisper a message or sound, of up to 25 words, into the air. A breeze carries that message at a speed of up to 6 miles per hour to a location that you have visited and is on the same plane as you. When the wind arrives, it audibly conveys your message or sound whether or not anyone is present to hear it." compset="Ritual">
