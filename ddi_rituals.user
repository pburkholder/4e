    </thing>
  <thing id="rtEvardExt" name="Evard&apos;s Extensible Rope" description="Your rope of climbing lengthens until it is as much as 1,000 feet long." compset="Ritual">
    <fieldval field="ritCompCst" value="Focus (rope of climbing)"/>
