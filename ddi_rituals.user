    <tag group="ReqLevel" tag="2"/>
    <tag group="RitualSkl" tag="skNature"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtPreseFla" name="Preserve Flame" description="You alter one fire of bonfire size or smaller within 5 squares of you. No amount of water or effort can douse the flame until the ritual&apos;s duration expires. The ritual caster can voluntarily end this effect as a minor action." compset="Ritual">
    <fieldval field="ritCompCst" value="75 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="8 hours"/>
    <fieldval field="ritPrice" value="360"/>
