    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtPanthTea" name="Panther Tears" description="An eyedropper holds a few drops of a dark purple fluid. When the fluid is placed in your eyes, you find you can see clearly in dim light." compset="Ritual">
