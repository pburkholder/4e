    <tag group="ReqLevel" tag="6"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtExploFir" name="Explorer&apos;s Fire" description="You alter one fire of bonfire size or smaller within 5 squares of you. Light cast by the fire cannot be seen from more than 5 squares from the fire. The fire&apos;s light is unchanged within that radius, and the ritual does not diminish or contain the fire&apos;s heat or sounds." compset="Ritual">
    <fieldval field="ritCompCst" value="10 gp"/>
    <fieldval field="ritTime" value="10 minutes"/>
    <fieldval field="ritLasts" value="8 hours"/>
    <fieldval field="ritPrice" value="50"/>
