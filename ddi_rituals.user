    <tag group="ReqLevel" tag="12"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtVistaPas" name="Vistani Passkey" description="You examine a lock or trap as part of the ritual and sprinkle residuum on the focus (see below). After enacting this ritual, you can use the focus to make a Thievery check with a +5 bonus on the lock or trap you examined. Even if you fail by 5 or more, you cannot set off a trap with a check augmented by the bonus from this ritual.{br}Focus: An ornate key or thieves&apos; tool worth 100 gp." compset="Ritual">
    <fieldval field="ritCompCst" value="100 gp, plus a focus worth 100 gp"/>
    <fieldval field="ritTime" value="Standard action"/>
    <fieldval field="ritLasts" value="See below"/>
    <fieldval field="ritPrice" value="250"/>
    <usesource source="Dragon"/>
