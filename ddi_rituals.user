    <tag group="ReqLevel" tag="7"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtGoodnTin" name="Goodnight Tincture" description="This liquid is dissolved into the food or drink of an unsuspecting victim to knock the subject unconscious." compset="Ritual">
    <fieldval field="ritCompCst" value="See Alchemical Item"/>
    <fieldval field="ritTime" value="1 hour"/>
