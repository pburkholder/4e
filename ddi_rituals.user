    <tag group="RitualCat" tag="Travel"/>
    <tag group="ReqLevel" tag="6"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtPorteWea" name="Portend Weather" description="You determine what the weather will be like for the next 24 hours within a 50-mile radius of you. You and any allies present for the ritual gain a +5 bonus to Endurance checks to endure extreme weather you have foreseen." compset="Ritual">
