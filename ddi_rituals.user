    <tag group="RitualCat" tag="Warding"/>
    <tag group="ReqLevel" tag="7"/>
    <tag group="RitualSkl" tag="skArcana"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtPurgeSpe" name="Purge Spellplague" description="You remove the effects of the Spellplague from the land around you. Your Arcana check result determines the radius of the area affected. That region is cleansed of the Spellplague, though there is no guarantee it will remain so.{br}{br}Arcana Check Result - Radius{br}24 or lower - 50 feet{br}25-29 - 500 feet{br}30-34 - 1 mile{br}35-39 - 5 miles{br}40 or higher - 10 miles" compset="Ritual">
    <fieldval field="ritCompCst" value="25,000 gp"/>
    <fieldval field="ritTime" value="30 minutes"/>
    <fieldval field="ritLasts" value="Instantaneous"/>
    <fieldval field="ritPrice" value="150000"/>
    <usesource source="FRPG"/>
    <tag group="ReqLevel" tag="24"/>
    <tag group="RitualCat" tag="Explor"/>
    <tag group="RitualSkl" tag="skArcana"/>
    </thing>
  <thing id="rtPurifSpe" name="Purify Spellscarred" description="You remove all traces of the Spellplague from a willing spellscarred creature. A spellscarred creature reverts to its normal appearance and loses its spellscar as well as all beneficial or harmful effects the scar provided. A character who has gained the Student of the Spellplague feat, or any other feats that require a creature to be spellscarred, loses those feats and can choose others to replace them." compset="Ritual">
    <fieldval field="ritCompCst" value="7,000 gp"/>
    <fieldval field="ritTime" value="30 minutes"/>
    <fieldval field="ritLasts" value="Instantaneous"/>
    <fieldval field="ritPrice" value="20000"/>
    <usesource source="FRPG"/>
    <tag group="RitualCat" tag="Restor"/>
    <tag group="ReqLevel" tag="18"/>
    <tag group="RitualSkl" tag="skHeal"/>
    <tag group="RitualSkl" tag="NoCheck"/>
    </thing>
  <thing id="rtPurifWat" name="Purify Water" description="You purify a volume of water. The amount of water you can purify is determined by the result of your Arcana check, Nature check, or Religion check.{br}{br}Skill Check Result - Amount of Water{br}9 or lower - 1 square{br}10-19 - 2 squares{br}20-34 - 4 squares{br}35 or higher - 10 squares" compset="Ritual">
