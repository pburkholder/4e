    </thing>
  <thing id="fRBlAciRes" name="Acid Resistance" description="You have resist acid 5 + one-half your level." compset="RaceFeat">
    <tag group="Hide" tag="Special"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      ~resist Fire 5 + half level
      var bonus as number
      bonus = 5 + round(hero.tagvalue[Level.?] / 2, 0, -1)
      #traitmodify[rsAcid,rsTotal,bonus,""]]]></eval>
    </thing>
  <thing id="fRBuOversi" name="Oversized" description="You can use weapons of your size or one size larger than you as if they were your size." compset="RaceFeat">
