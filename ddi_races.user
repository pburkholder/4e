    <tag group="Hide" tag="Special"/>
    </thing>
  <thing id="fRGeEleOri" name="Elemental Origins" description="Your ancestors were native to the Elemental Chaos, so you are considered an elemental creature for the purpose of effects that relate to creature origin." compset="RaceFeat"></thing>
  <thing id="fRGiDanSen" name="Danger Sense" description="You have a +2 racial bonus to initiative checks." compset="RaceFeat"></thing>
  <thing id="fRGiGitWil" name="Githyanki Willpower" description="You gain a +1 bonus to your Will defense, and a +2 bonus to saving throws against charm effects." compset="RaceFeat"></thing>
  <thing id="fRGnBloFur" name="Blood Fury" description="While you&apos;re bloodied, you gain a +2 bonus to damage rolls. This increases to a +4 bonus at 21st level." compset="RaceFeat"></thing>
  <thing id="fRGnPacAtt" name="Pack Attack" description="You deal an extra 2 damage on melee attacks against an enemy that has two or more of your allies adjacent to it." compset="RaceFeat"></thing>
  <thing id="fRGnFeyOri" name="Fey Origin" description="Your ancestors were native to the Feywild, so you are considered a fey creature for the purpose of effects that relate to creature origin." compset="RaceFeat"></thing>
  <thing id="fRGnMasTri" name="Master Trickster" description="Once per encounter, you can use the wizard cantrip ghost sound as a minor action." compset="RaceFeat">
